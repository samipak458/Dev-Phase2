    /// <summary>
    /// Set the range of claim ID
    /// </summary>
    public void init()
    {
        Args            args;
        Query           query;
        QueryBuildRange qr;
        HMClaimTable    claimTable;
        QueryBuildDataSource qbds, qbdsIntegrationStatus;

        super();

        args = element.args();

        claimTable = args.record() as HMClaimTable;

        if(args.record() && args.record().TableID == tableNum(HMClaimTable))
        {  
            qr = HMClaimChargeLine_ds.queryBuildDataSource().addRange(fieldNum(HMClaimChargeLine, ClaimID));
            qr.value(queryValue(claimTable.ClaimID));
            qr.status(RangeStatus::Locked);            
        }
    }

]]></Source>
