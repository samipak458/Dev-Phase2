			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();

        filterPane = this.control(this.controlId(FilterPaneControl::DefaultName()));

        QueryBuildDataSource qbdsHMClaimTable = HMClaimTable_ds.query().dataSourceName(HMClaimTable_ds.name());
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, ClaimID)));
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, HMClaimDate)));
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, HMSite)));
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, HMBalanceAmount)));
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, PatientName)));

        QueryBuildDataSource qbdsHMClaimTablePBMUBO4Details = HMClaimTable_ds.query().dataSourceName(HMClaimTablePBMUBO4Details_ds.name());
        SysQuery::findOrCreateRange(qbdsHMClaimTablePBMUBO4Details,(fieldNum(HMClaimTablePBMUBO4Details, PrescriptionNumber)));

        QueryBuildDataSource qbdsHMClaimPaymentPosting = HMClaimTable_ds.query().dataSourceName(HMClaimPaymentPosting_ds.name());
        SysQuery::findOrCreateRange(qbdsHMClaimPaymentPosting,(fieldNum(HMClaimPaymentPosting, Code_Cheque)));
        SysQuery::findOrCreateRange(qbdsHMClaimPaymentPosting,(fieldNum(HMClaimPaymentPosting, Type)));

        QueryBuildDataSource qbdsHMInsuranceCarrier = HMClaimTable_ds.query().dataSourceName(HMInsuranceCarrier_ds.name());
        SysQuery::findOrCreateRange(qbdsHMInsuranceCarrier,(fieldNum(HMInsuranceCarrier, Code)));

        //QueryBuildDataSource qbdsHMPatient = HMClaimTable_ds.query().dataSourceName(HMPatient_ds.name());
        //QueryBuildDataSource qbdsCustTable = CustTable_ds.query().dataSourceName(CustTable_ds.name());
        //qbdsCustTable.relations(false);
        //qbdsCustTable.joinMode(JoinMode::InnerJoin);
        //qbdsCustTable.clearLinks();
        //qbdsCustTable.clearRanges();
        //qbdsCustTable.addLink(fieldnum(HMPatient, CustTable),fieldnum(CustTable, RecId));
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, PatientAccountNum)));

        if (element.args().menuItemName() == menuitemDisplayStr(HMAllClaimsInquiryExcludePaymentPosting))
        {
            ExcludePaymentPostingDS.checked(true);
            HMClaimTable_ds.query().dataSourceTable(tableNum(HMClaimPaymentPosting)).enabled(false);
        }
    }

]]></Source>
