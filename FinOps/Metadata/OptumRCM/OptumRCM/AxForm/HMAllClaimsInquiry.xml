				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            element.close();

            Args    args;
            FormRun formRun;

            super();

            args = new Args();

            if(ExcludePaymentPostingDS.value() == 1)
            {
                formRun = new menufunction(menuItemDisplayStr(HMAllClaimsInquiryExcludePaymentPosting), MenuItemType::Display).create(args);
            }
            else
            {
                formRun = new menufunction(menuItemDisplayStr(HMAllClaimsInquiry), MenuItemType::Display).create(args);
            }
            formRun.init();
            formRun.run();
            formRun.wait();
        }

]]></Source>
