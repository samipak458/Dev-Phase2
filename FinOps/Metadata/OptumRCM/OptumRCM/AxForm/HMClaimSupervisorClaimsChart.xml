			<Method>
				<Name>buildTempTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Builds the data in the temp table to display on the graph
    /// </summary>
    /// <param name = "_update">True if updating the temp table with new values</param>
    /// <remarks>If _update is true the method first deletes all records from the temp table then inserts the new calculated values</remarks>
    public void buildTempTable()
    {
        int                     claimCount = 0;
        Query                   query;
        QueryRun                qr;
        QueryBuildDataSource    qbds;
        str                     status;

        delete_from bucket; 

     
        //Get count for aging period 0 - 30 days
        query = new Query(queryStr(HMClaimSupervisorAllPoolClaimsAging0_30Query));
        qr = new QueryRun(query);
        claimCount = SysQuery::countLoops(qr);
        status = "a. "+"@Optum:HMClaimAging0To30Days";   
        this.insertDataIntoBucket(status, claimCount);
        //End  
      
        //Get count for aging period 31 - 60 days
        query = new Query(queryStr(HMClaimSupervisorAllPoolClaimsAging30_60Query));
        qr = new QueryRun(query);
        claimCount = SysQuery::countLoops(qr);
        status = "b. "+"@MAZ:HMClaimAging30To60Days";
        this.insertDataIntoBucket(status, claimCount);
        //End  
       
        //Get count for aging period 61 - 90 days
        query = new Query(queryStr(HMClaimSupervisorAllPoolClaimsAging60_90Query));
        qr = new QueryRun(query);
        claimCount = SysQuery::countLoops(qr);
        status = "c. "+"@MAZ:HMClaimAging61To90Days";
        this.insertDataIntoBucket(status, claimCount);
        //End         

        //Get count for aging period 91 - 120 days
        query = new Query(queryStr(HMClaimSupervisorAllPoolClaimsAging90_120Query));
        qr = new QueryRun(query);
        claimCount = SysQuery::countLoops(qr);
        status = "d. "+"@MAZ:HMClaimAging91To120Days";
        this.insertDataIntoBucket(status, claimCount);
        //End
      
        //Get count for aging period > 120 days
        query = new Query(queryStr(HMClaimSupervisorAllPoolClaimsAging120AboveQuery));
        qr = new QueryRun(query);
        claimCount = SysQuery::countLoops(qr);
        status = "e. "+"@MAZ:HMClaimAging120PlusDays";
        this.insertDataIntoBucket(status, claimCount);
        //End          
    
        //Get count for Total active claims with assigned agent
        query = new Query(queryStr(HMClaimSupervisorAllPoolAssignedClaimsQuery));
        qr = new QueryRun(query);
        claimCount = SysQuery::countLoops(qr);
        status = "f. "+"@Optum:HMClaimsAssignedToMyAgents";
        this.insertDataIntoBucket(status, claimCount);
        //End
        
        mzkClaimManagerChartDataTmp.linkPhysicalTableInstance(bucket);
    }

]]></Source>
			</Method>
			<Method>
				<Name>insertDataIntoBucket</Name>
				<Source><![CDATA[
    public void insertDataIntoBucket(str _status, int _counts)
    {
        bucket.clear();
        bucket.ClaimCount = _counts;
        bucket.Status = _status;
        bucket.insert();
    }

]]></Source>
