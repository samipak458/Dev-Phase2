			<Method>
				<Name>swapPriority</Name>
				<Source><![CDATA[
    public void swapPriority(int _swapingVal)
    {
        HMClaimPriorityTable    claimPriorityTable;
        HMPriority              savePreviousPriority;
        RecId                   claimPriorityTableRecId;

        select forupdate claimPriorityTable
            where claimPriorityTable.Priority == HMClaimPriorityTable.Priority + _swapingVal
                && claimPriorityTable.HMPriorityStrategyID == HMClaimPriorityTable.HMPriorityStrategyID;

        savePreviousPriority    = HMClaimPriorityTable.Priority;
        claimPriorityTableRecId = HMClaimPriorityTable.RecId;

        try
        {
            ttsbegin;

            claimPriorityTable.Priority = maxPriority.Priority + 1;
            claimPriorityTable.update();
            
            HMClaimPriorityTable.Priority = HMClaimPriorityTable.Priority + _swapingVal;
            HMClaimPriorityTable.update();

            claimPriorityTable.Priority = savePreviousPriority;
            claimPriorityTable.update();
            
            ttscommit;
        }
        catch
        {
            warning('@Optum:UnableToSwapPriority');
        }
        
        HMClaimPriorityTable_ds.executeQuery();
        HMClaimPriorityTable_ds.findValue(fieldNum(HMClaimPriorityTable, recId), int642str(claimPriorityTableRecId));
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        str                     priorityStrategyID;
        HMPriorityStrategy      hmPriorityStrategy;
        QueryBuildDataSource    qbds;

        super();

        if(element.args() && element.args().menuItemName() == menuItemDisplayStr(HMRelatedPriorities))
        {
            hmPriorityStrategy = element.args().record();

            if(hmPriorityStrategy.RecId != 0)
            {
                priorityStrategyID = hmPriorityStrategy.HMPriorityStrategyID;

                qbds = HMClaimPriorityTable_DS.queryBuildDataSource();
                qbds.addRange(fieldnum(HMClaimPriorityTable, HMPriorityStrategyID)).value(queryvalue(priorityStrategyID));
            }
        }
        else
        {
            element.design(0).controlName(FormControlStr(CustCollectionsPool, MoveUp)).visible(false);
            element.design(0).controlName(FormControlStr(CustCollectionsPool, MoveDown)).visible(false);
        }
    }

]]></Source>
