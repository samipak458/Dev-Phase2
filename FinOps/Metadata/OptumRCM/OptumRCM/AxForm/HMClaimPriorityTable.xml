        void clicked()
        {
            SysQueryRun queryRun;

            if (HMClaimPriorityTable.RecId)
            {
                queryRun = new SysQueryRun(new Query(HMClaimPriorityTable.QueryPacked));

                queryRun.args().caller(element); // if caller isn't a form, the title isn't displayed!
                queryRun.title("@Optum:MAZOPT0078");
                queryRun.promptShowSorting(false);
                queryRun.promptShowReset(false);

                if (queryRun.prompt())
                {
                    HMClaimPriorityTable.QueryPacked = queryRun.query().pack();
                }

                element.refreshCriteria();
            }
        }

]]></Source>
