					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret = super();

            element.refreshCriteria();

            select minof(Priority) from minPriority
                where minPriority.HMPriorityStrategyID == HMClaimPriorityTable.HMPriorityStrategyID;

            select maxof(Priority) from maxPriority
                where maxPriority.HMPriorityStrategyID == HMClaimPriorityTable.HMPriorityStrategyID;

            if(minPriority.Priority == HMClaimPriorityTable.Priority)
            {
                MoveUp.enabled(false);
                MoveDown.enabled(true);
            }
            else if(maxPriority.Priority == HMClaimPriorityTable.Priority)
            {
                MoveDown.enabled(false);
                MoveUp.enabled(true);
            }
            else
            {
                MoveUp.enabled(true);
                MoveDown.enabled(true);
            }

            return ret;
        }

]]></Source>
