			<Method>
				<Name>buildTempTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Builds the data in the temp table to display on the graph
    /// </summary>
    /// <param name = "_update">True if updating the temp table with new values</param>
    /// <remarks>If _update is true the method first deletes all records from the temp table then inserts the new calculated values</remarks>
    public void buildTempTable()
    {
        int                     claimCount = 0;
        Query                   query;
        QueryRun                qr;
        QueryBuildDataSource    qbds;
        str                     status;
        
        delete_from bucket;

        //Get count for Total active claims in my pool
        query = new Query(queryStr(HMClaimManagerAllPoolsClaimQuery));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "a. "+"@MAZ:HMTotalActiveClaims";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Start get count for claim with no assigned pool
        query = new Query(queryStr(HMClaimWorkspacesTileUnassignedPoolQuery));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countTotal(qr);
        status = "b. "+"@MAZ:HMClaimsWithNoAssignedPool";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Start get count for claim with no assigned agent
        query = new Query(queryStr(HMClaimManagerAllPoolsUnAssignedClaimQuery));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "c. "+"@MAZ:HMClaimsWithNoAssignedAgent";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for Claims with assigned agent
        query = new Query(queryStr(HMClaimManagerAllPoolsAssignedClaimQuery));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "d. "+"@MAZ:HMClaimsWithAssignedAgents";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for aging period 0 - 30 days
        query = new Query(queryStr(HMClaimManagerAllPoolsClaimsAging0_30Query));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "e. "+"@Optum:HMClaimAging0To30Days";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for aging period 31 - 60 days
        query = new Query(queryStr(HMClaimManagerAllPoolsClaimAging30_60Query));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "f. "+"@MAZ:HMClaimAging30To60Days";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for aging period 61 - 90 days
        query = new Query(queryStr(HMClaimManagerAllPoolsClaimAging60_90Query));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "g. "+"@MAZ:HMClaimAging61To90Days";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for aging period 91 - 120 days
        query = new Query(queryStr(HMClaimManagerAllPoolsClaimAging90_120Query));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "h. "+"@MAZ:HMClaimAging91To120Days";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for aging period > 120 days
        query = new Query(queryStr(HMClaimManagerAllPoolsClaimAging120AboveQuery));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "i. "+"@MAZ:HMClaimAging120PlusDays";

        this.insertDataIntoBucket(status, claimCount);
        //End

        //Get count for claim with no active notes 7+ days
        query = new Query(queryStr(HMClaimManagerNoNotesClaimQuery));

        qr = new QueryRun(query);
        
        claimCount = SysQuery::countLoops(qr);
        status = "j. "+"@MAZ:HMClaimWithNoActiveNotes";

        this.insertDataIntoBucket(status, claimCount);
        //End

        mzkClaimManagerChartDataTmp.linkPhysicalTableInstance(bucket);
    }

]]></Source>
