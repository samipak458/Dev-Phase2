			<Method>
				<Name>insertDataIntoBucket</Name>
				<Source><![CDATA[
    public void insertDataIntoBucket(str _status, int _counts)
    {
        bucket.clear();
        bucket.ClaimCount = _counts;
        bucket.Status = _status;
        bucket.insert();
    }

]]></Source>
