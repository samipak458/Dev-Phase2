				<Source><![CDATA[
[Form]
public class HMSummaryAmount extends FormRun
{
    HMCase              hmcase;
    HMClaimTable        hmClaimTable;
    HMClaimTableDetails hmClaimTableDetails;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        super();
        hmcase = element.args().record();
        if (!hmcase)
        {
            warning("@Optum:NoCaseWarning");
        }
        select sum(ClaimAmount),sum(HMExpectedAmount) from hmClaimTable
            where  hmClaimTable.CaseNo == HMCase.CaseNo
                join sum(HMAmountPaid),sum(HMBalanceAmount),sum(RevisedAmount) from hmclaimTableDetails
                    where hmClaimTableDetails.HMClaimTable == hmClaimTable.RecId;
        
        TotalClaimAmount.realValue(hmClaimTable.ClaimAmount);
        TotalExpectedAmount.realValue(hmClaimTable.HMExpectedAmount);
        TotalPaidAmount.realValue(hmClaimTableDetails.HMAmountPaid);
        TotalBalanceAmount.realValue(hmClaimTableDetails.HMBalanceAmount);
        TotalAdjustedAmount.realValue(hmClaimTableDetails.RevisedAmount);
    }

]]></Source>
			</Method>
		</Methods>
