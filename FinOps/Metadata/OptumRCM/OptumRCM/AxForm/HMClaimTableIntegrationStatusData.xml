    /// <summary>
    /// Set the range of claim ID
    /// </summary>
    public void init()
    {
        Args            args;
        HMClaimTable    claimTable;
        QueryBuildRange qr;

        super();

        args = element.args();

        claimTable = args.record() as HMClaimTable;

        if(args.record() && args.record().TableID == tableNum(HMClaimTable))
        {
            qr = HMClaimTableIntegrationStatusData_ds.queryBuildDataSource().addRange(fieldNum(HMClaimTableIntegrationStatusData, ClaimID));
            qr.value(queryValue(claimTable.ClaimID));
            qr.status(RangeStatus::Locked);
        }
    }

