			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        filterPane = this.control(this.controlId(FilterPaneControl::DefaultName()));

        QueryBuildDataSource qbdsUserInfo = HMClaimNotes_ds.query().dataSourceName(UserInfo_ds.name());
        QueryBuildRange userInfoFilter = SysQuery::findOrCreateRange(qbdsUserInfo, (fieldNum(UserInfo, Name)));
        userInfoFilter.label("@Optum:HMUserCreatedBy");

        QueryBuildDataSource qbdsHMClaimNotes = HMClaimNotes_ds.query().dataSourceName(HMClaimNotes_ds.name());
        SysQuery::findOrCreateRange(qbdsHMClaimNotes,(fieldNum(HMClaimNotes, CreatedDateTime)));
        SysQuery::findOrCreateRange(qbdsHMClaimNotes,(fieldNum(HMClaimNotes, HMFollowUp)));
        SysQuery::findOrCreateRange(qbdsHMClaimNotes,(fieldNum(HMClaimNotes, HMClaimProcessStatus)));
        SysQuery::findOrCreateRange(qbdsHMClaimNotes,(fieldNum(HMClaimNotes, HMActionType)));
        
        QueryBuildDataSource qbdsHMClaimTable = HMClaimNotes_ds.query().dataSourceName(HMClaimTable_ds.name());
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, ClaimID)));
        SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, HMClaimDate)));
        
        QueryBuildDataSource qbdsHMInsuranceCarrier = HMClaimTable_ds.query().dataSourceName(HMInsuranceCarrier_ds.name());
        SysQuery::findOrCreateRange(qbdsHMInsuranceCarrier,(fieldNum(HMInsuranceCarrier, Code)));

        QueryBuildRange patientFilter = SysQuery::findOrCreateRange(qbdsHMClaimTable,(fieldNum(HMClaimTable, PatientName)));
        patientFilter.label("@MAZ367");
    }

]]></Source>
			</Method>
			<Method>
				<Name>isExpressionEmpty</Name>
				<Source><![CDATA[
    public boolean isExpressionEmpty()
    {
        FieldFilterExpression expression;
        List tempList =  filterPane.parmFilterExpressionList();
        
        ListIterator listIterator = new ListIterator(tempList);
        while(listIterator.more())
        {
            expression = listIterator.value();
            if(expression.parmValueAsString())
            {
                return false;
            }
            listIterator.next();
        }
        return true;
    }

]]></Source>
