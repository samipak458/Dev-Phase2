			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    /// Set the range of claim ID
    /// </summary>
    public void init()
    {
        Args            args;
        HMClaimTable    claimTable;
        QueryBuildDataSource qbds;

        super();

        args = element.args();

        claimTable = args.record() as HMClaimTable;

        if(args.record() && args.record().TableID == tableNum(HMClaimTable))
        {
            qbds = HMAdjudicationHeader_ds.queryBuildDataSource();
            qbds.addRange(fieldNum(HMAdjudicationHeader, InvoiceNumber)).value(queryValue(claimTable.CaseNo));
            qbds.addRange(fieldNum(HMAdjudicationHeader, HMInsurance)).value(queryValue(claimTable.HMInsuranceCarrier));
        }
    }

]]></Source>
			</Method>
