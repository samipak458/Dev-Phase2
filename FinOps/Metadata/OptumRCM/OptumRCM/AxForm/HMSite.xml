						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        /// <returns></returns>
        public boolean validateWrite()
        {
            boolean     ret;
            str         dialogMsg = "@Optum:UpdateClaimCacheFieldsConfirmationMsg";
            str         dialogDescription = "@Optum:UpdateClaimCacheFieldDescriptionConfirmationMsg";
            str         dialogTitle = "@SYS4006395";
            Name        oldValue;
            HMSiteDescription OldValueSiteDescription;

            HMSiteTable site = this.cursor();

            oldValue                = site.orig().HMRegion;
            OldValueSiteDescription = site.orig().HMDescription;

            ret = super();

            if(oldValue != site.HMRegion)
            {
                if(Box::yesNo(dialogMsg ,DialogButton::Yes ,dialogTitle) == DialogButton::No)
                {
                    ret = false;
                }
            }
            else if (OldValueSiteDescription != site.HMDescription)
            {
                if (Box::yesNo(dialogDescription, DialogButton::Yes, dialogTitle) == DialogButton::No)
                {
                    ret = false;
                }
            }
        
            return ret;
        }

]]></Source>
