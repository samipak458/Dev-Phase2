				<Source><![CDATA[
    /// <summary>
    /// Returns the claimProcessStatus table buffer. 
    /// </summary>
    static HMClaimProcessStatus find(str _status,
                          boolean _forUpdate = false)
    {
        HMClaimProcessStatus hmClaimProcessStatus;

        if (_status)
        {
            if (_forUpdate)
            {
                hmClaimProcessStatus.selectForUpdate(_forUpdate);
            }

            select firstonly hmClaimProcessStatus
                where hmClaimProcessStatus.Status == _status;
        }

        return hmClaimProcessStatus;
    }

]]></Source>
