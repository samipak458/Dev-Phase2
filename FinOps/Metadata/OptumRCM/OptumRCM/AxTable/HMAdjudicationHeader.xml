		<Methods>
			<Method>
				<Name>existAdjudicationHeader</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    static HMAdjudicationHeader existAdjudicationHeader(HMCaseNo _caseNo,RefRecId _insuranceRecId, boolean _forUpdate = false)
    {
        HMAdjudicationHeader claimAdjudicationHeader;

        if (_caseNo)
        {
            if (_forUpdate)
            {
                claimAdjudicationHeader.selectForUpdate(_forUpdate);
            }

            select firstonly TransNum from claimAdjudicationHeader
                where claimAdjudicationHeader.InvoiceNumber == _caseNo
                    && claimAdjudicationHeader.HMInsurance == _insuranceRecId;
        }

        return claimAdjudicationHeader;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
