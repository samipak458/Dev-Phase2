			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Find the existing record by <c>UserId</c>
    /// </summary>
    /// <param name="_userId"><c>UserId</c></param>
    /// <param name="_forUpdate">true if update needed</param>
    /// <returns><c>HMUserInfo</c> record</returns>
    static HMUserInfo find(UserId _userId,
                          boolean _forUpdate = false)
    {
        HMUserInfo hmUserInfo;

        if (_userId)
        {
            if (_forUpdate)
            {
                hmUserInfo.selectForUpdate(_forUpdate);
            }

            select firstonly hmUserInfo
                index hint Idx
                where hmUserInfo.UserId == _userId;
        }

        return hmUserInfo;
    }

]]></Source>
