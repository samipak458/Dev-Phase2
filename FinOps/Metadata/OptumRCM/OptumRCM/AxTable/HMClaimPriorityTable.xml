			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        if ((select firstonly hmClaimTable where hmClaimTable.HMClaimPriority == this.Priority && hmClaimTable.HMPriorityStrategyID == this.HMPriorityStrategyID).RecId)
        {
            error("@Optum:MAZOPT0076");
            return false;
        }

        return super();
    }

]]></Source>
