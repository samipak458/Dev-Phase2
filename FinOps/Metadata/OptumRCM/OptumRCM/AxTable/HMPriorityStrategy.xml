				<Name>validateDelete</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates the delete of <c>HMPriorityStrategy</c>
    /// </summary>
    /// <returns>Returns true if the validation succeeds.</returns>
    public boolean validateDelete()
    {
        boolean ret;
        CustCollectionsPool     custCollectionsPool;
        HMClaimPriorityTable    hmClaimPriorityTable;
    
        ret = super();

        select firstonly RecId,Name from custCollectionsPool
            where custCollectionsPool.HMPriorityStrategyID == this.HMPriorityStrategyID;

        select firstonly RecId,Priority from hmClaimPriorityTable
            where hmClaimPriorityTable.HMPriorityStrategyID == this.HMPriorityStrategyID;

        if (custCollectionsPool.RecId != 0 || hmClaimPriorityTable.RecId != 0)
        {
            ret = checkFailed("@Optum:HMPriorityStrategyDeletion");
        }
            
        return ret;
    }

]]></Source>
