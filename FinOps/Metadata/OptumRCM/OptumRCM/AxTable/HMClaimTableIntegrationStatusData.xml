		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Find the <c>HMClaimTableIntegrationStatusData</c> record by claim number
    /// </summary>
    /// <param name="_claimId">Claim number</param>
    /// <param name="_forUpdate">If yes use buffer as update</param>
    /// <returns><c>HMClaimTableIntegrationStatusData</c> record</returns>
    static HMClaimTableIntegrationStatusData find(HMClaimNumber _claimId, boolean _forUpdate = false)
    {
        HMClaimTableIntegrationStatusData claimTableIntegrationStatusData;

        if (_claimId)
        {
            if (_forUpdate)
            {
                claimTableIntegrationStatusData.selectForUpdate(_forUpdate);
            }

            select firstonly claimTableIntegrationStatusData
                index hint ClaimIntegrationStatsIdx
                where claimTableIntegrationStatusData.ClaimID == _claimId;
        }

        return claimTableIntegrationStatusData;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findLatestRecordByClaimId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Find the latest <c>HMClaimTableIntegrationStatusData</c> record by claim number
    /// </summary>
    /// <param name="_claimId">Claim number</param>
    /// <param name="_forUpdate">If yes use buffer as update</param>
    /// <returns><c>HMClaimTableIntegrationStatusData</c> record</returns>
    static HMClaimTableIntegrationStatusData findLatestRecordByClaimId(HMClaimNumber _claimId, boolean _forUpdate = false)
    {
        HMClaimTableIntegrationStatusData claimTableIntegrationStatusData;

        if (_claimId)
        {
            if (_forUpdate)
            {
                claimTableIntegrationStatusData.selectForUpdate(_forUpdate);
            }

            select firstonly claimTableIntegrationStatusData
                order by claimTableIntegrationStatusData.LatestResponse desc
                where claimTableIntegrationStatusData.ClaimID == _claimId;
        }

        return claimTableIntegrationStatusData;
    }

