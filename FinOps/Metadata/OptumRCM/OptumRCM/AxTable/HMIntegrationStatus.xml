				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Find integration by name
    /// </summary>
    /// <param name="_name"><c>HMIntegrationStatusName</c></param>
    /// <param name="_forUpdate">If need to update</param>
    /// <returns><c>HMIntegrationStatus</c> record</returns>
    static HMIntegrationStatus find(HMIntegrationStatusName _name, boolean _forUpdate = false)
    {
        HMIntegrationStatus hmIntegrationStatus;

        if (_name)
        {
            if (_forUpdate)
            {
                hmIntegrationStatus.selectForUpdate(_forUpdate);
            }

            select firstonly hmIntegrationStatus
                index hint HMIntegrationStatusIdx
                where hmIntegrationStatus.Name == _name;
        }

        return hmIntegrationStatus;
    }

]]></Source>
