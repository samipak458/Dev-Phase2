				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    /// <returns></returns>
    public boolean validateDelete()
    {
        boolean             ret;
        HMActionTypeTable   hmActionTypeTable;

        select firstonly RecId from hmActionTypeTable
            where HMActionTypeTable.HMTemplateID == this.HMTemplateID;

        if(hmActionTypeTable.RecId)
        {
            ret =  checkFailed(strFmt('@Optum:DeleteValidationForHMActionAndNotesTemplates',this.HMTemplateID));
        }
        else
        {
            ret = super();
        }

        return ret;
    }

]]></Source>
