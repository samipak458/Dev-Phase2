    public static void postTargetProcess(DMFDefinitionGroupExecution _dmfDefinitionGroupExecution)
    {
        if (_dmfDefinitionGroupExecution.StagingStatus == DMFBatchJobStatus::Finished)
        {
            HMClaimActionNotesStaging   myStaging;
            HMClaimTable                hmClaims;
            HMClaimNotes                hmClaimNotes;

            ttsbegin;
                update_recordset hmClaims
                    setting
                    HMResponsibleGroup = myStaging.HMResponsibleGroup,
                    HMClaimProcessStatus = myStaging.HMClaimProcessStatus,
                    HMFollowUp = myStaging.HMFollowUp,
                    HMClaimLatestNote_Internal = myStaging.Notes,
                    HMClaimLatestNoteDate_Internal = myStaging.CreatedDateTime,
                    HMClaimLatestNoteCreatedBy_Internal = myStaging.CreatedBy,
                    HMActionType = myStaging.HMActionType
                        join myStaging
                        where
                        hmClaims.ClaimID == myStaging.ClaimID
                        && myStaging.DefinitionGroup == _dmfDefinitionGroupExecution.DefinitionGroup
                        && myStaging.ExecutionId     == _dmfDefinitionGroupExecution.ExecutionId
                        && myStaging.TransferStatus  == DMFTransferStatus::Completed;
            ttscommit;
        }
    }

]]></Source>
