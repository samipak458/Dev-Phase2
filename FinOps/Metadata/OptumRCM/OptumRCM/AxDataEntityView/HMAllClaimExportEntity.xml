				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public static str getWaystarStatusCode()
    {
        str waystarStatusCode;

        waystarStatusCode = strFmt(@"SELECT TOP 1 A.STATUSCODE from HMCLAIMTABLEINTEGRATIONSTATUSDATA AS A WHERE CLAIMID = %1 ORDER BY A.LATESTRESPONSE DESC",
            SysComputedColumn::returnField(dataentityviewstr(HMAllClaimExportEntity), dataEntityDataSourceStr(HMAllClaimExportEntity, HMAllClaimExport), fieldStr(HMAllClaimExport, ClaimId)));

        return waystarStatusCode;
    }

]]></Source>
