			<Method>
				<Name>getWaystarStatusType</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public static str getWaystarStatusType()
    {
        DictEnum    dictEnum = new DictEnum(enumNum(HMIntegrationStatusType));
        Map         enumValues = new Map(Types::String, Types::String);
        int         n;
        str         waystarStatusType;
        str         cases = 'Case ';

        for (n = 0; n < dictEnum.values(); n++)
        {
            cases += strFmt("when B.TYPE = %1 then %2", int2str(dictEnum.index2Value(n)), SysComputedColumn::returnLiteral(dictEnum.index2Symbol(n)));

            if(dictEnum.values() == n + 1)
            {
                cases += ' end';
            }
        }

        waystarStatusType = strFmt(@"SELECT TOP 1 %2 from HMCLAIMTABLEINTEGRATIONSTATUSDATA AS A INNER JOIN HMINTEGRATIONSTATUS AS B ON B.NAME = A.STATUSCODE WHERE A.CLAIMID = %1 ORDER BY A.LATESTRESPONSE DESC"
            , SysComputedColumn::returnField(dataentityviewstr(HMAllClaimExportEntity), dataEntityDataSourceStr(HMAllClaimExportEntity, HMAllClaimExport), fieldStr(HMAllClaimExport, ClaimId))
            , cases);

        return waystarStatusType;
    }

]]></Source>
