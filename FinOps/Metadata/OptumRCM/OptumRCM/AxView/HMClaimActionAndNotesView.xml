		<Methods>
			<Method>
				<Name>getCreatedDateTimeCST</Name>
				<Source><![CDATA[
    public static str getCreatedDateTimeCST()
    {
        str createdDateTimeCST;

        createdDateTimeCST = strFmt(@"SELECT DATEADD(hour, -6, CREATEDDATETIME) FROM HMCLAIMNOTES WHERE RECID = %1",
            SysComputedColumn::returnField(tableStr(HMClaimActionAndNotesView), identifierStr(HMClaimNotes), fieldStr(HMClaimNotes, RecId)));

        return createdDateTimeCST;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getAgentGoal</Name>
				<Source><![CDATA[
    public static str getAgentGoal()
    {
        str agentGoal;

        agentGoal = strFmt(@"SELECT A.HMGOAL FROM CUSTCOLLECTIONSAGENT AS A INNER JOIN dbo.DIRPARTYRELATIONSHIP AS B ON A.DIRPARTYRELATIONSHIP = B.RECID AND B.RELATIONSHIPTYPEID = N'TeamMember' AND B.VALIDTO >= '12/31/2154' AND B.CHILDPARTY = %1",
            SysComputedColumn::returnField(tableStr(HMClaimActionAndNotesView), identifierStr(DirPartyTable), fieldStr(DirPartyTable, RecId)));

        return agentGoal;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getAgentSupervisor</Name>
				<Source><![CDATA[
    public static str getAgentSupervisor()
    {
        str agentSupervisor;

        agentSupervisor = strFmt(@"SELECT A.NAME FROM DIRPARTYTABLE AS A INNER JOIN CUSTCOLLECTIONSAGENT AS B ON B.HMSUPERVISOR = A.RECID INNER JOIN dbo.DIRPARTYRELATIONSHIP AS C ON C.RECID = B.DIRPARTYRELATIONSHIP AND C.RELATIONSHIPTYPEID = N'TeamMember' AND C.VALIDTO >= '12/31/2154' AND C.CHILDPARTY = %1",
            SysComputedColumn::returnField(tableStr(HMClaimActionAndNotesView), identifierStr(DirPartyTable), fieldStr(DirPartyTable, RecId)));

        return agentSupervisor;
    }

]]></Source>
			</Method>
		</Methods>
