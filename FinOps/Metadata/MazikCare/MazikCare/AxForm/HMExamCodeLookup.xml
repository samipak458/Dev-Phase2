        void clicked()
        {
            FormRun formRun;
            Object object;
        
            super();
        
            formRun = element.args().caller();
        
            if(formHasMethod(formRUn,'setProcedureLocal'))
            {
                object = formRun;
                object.setProcedureLocal(InventTable.ItemId,element.args().parm());
            }
            else if(formHasMethod(formRun,'storeItem'))
            {
                object = formRun;
                object.storeItem(InventTable.ItemId);
            }
        
            okCalled = true;
            element.close();
        }

