								<Name>modified</Name>
								<Source><![CDATA[
            public void modified()
            {
                HMRegistrationRequestBirthDate calcBirthDate;
                HMAgeHelper         hmAgeHelper = new HMAgeHelper();
            
                super();
            
                HMRegistrationRequest.BirthDate = dateNull();
                calcBirthDate = hmAgeHelper.getDateFromAge(HMRegistrationRequest.Age);
                HMRegistrationRequest.BirthDate = calcBirthDate;
                if (calcBirthDate == dateNull())
                {
                    HMRegistrationRequest.Age = 0;
                }
            }

]]></Source>
