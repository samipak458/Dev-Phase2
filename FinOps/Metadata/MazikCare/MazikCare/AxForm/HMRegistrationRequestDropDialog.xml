				<Source><![CDATA[
    public void closeOk()
    {
        registrationRequest.parmAge(HMRegistrationRequest.Age);
        registrationRequest.parmBirthDate(HMRegistrationRequest.BirthDate);
        registrationRequest.parmContact(HMRegistrationRequest.Contact);
        registrationRequest.parmGender(HMRegistrationRequest.Gender);
        registrationRequest.parmName(HMRegistrationRequest.Name);
        registrationRequest.parmNationalId(HMRegistrationRequest.NationalId);
        registrationRequest.parmRequestNumber(HMRegistrationRequest.RequestNumber);
        registrationRequest.parmTokenNumber(HMRegistrationRequest.TokenNumber);
    
        if(HMRegistrationRequest.validateWrite() && registrationRequest.createRequest())
        {
            element.numberSeqFormHandler().formMethodDataSourceWrite();
    
            super();
        }
    }

]]></Source>
