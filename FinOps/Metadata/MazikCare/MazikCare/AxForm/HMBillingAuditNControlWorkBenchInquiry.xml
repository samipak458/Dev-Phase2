					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            Query q;
            QueryBuildRange qbr;
            QueryBuildDataSource qbds;
            QueryBuildDataSource qbdsSearchFIeld;
            QueryRun qr;
            HMInsuranceCarrierCode insuranceCarrierCode;
        
            super();
        
            if (BatchNumber.valueStr())
            {
                element.clearRanges();
        
                q = HMClaimChargeLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMClaimChargeLine));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::OuterJoin);
                
                // Musab
                q = HMCaseInsurance_ds.query();
                qbds = q.dataSourceTable(tablenum(HMCaseInsurance));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::InnerJoin);
        
                q = HMPatientChargeEntryLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMPatientChargeEntryLine));
                qbds.clearRanges();
        
                qbdsSearchField = q.dataSourceTable(tablenum(HMPatientChargeEntryLine));
        
                qbr = qbdsSearchField.addRange(fieldnum(HMPatientChargeEntryLine, HMInsuranceCarrierBatchDetails));
                qbr.value(strFmt('%1',HMInsuranceCarrierBatchDetails::findbByBatchNumber(BatchNumber.valueStr()).RecId));
                qr = new QueryRun(q);
        
                HMPatientChargeEntryTable_ds.query(qr.query());
        
                HMPatientChargeEntryTable_ds.executeQuery();
        
            }
            else if (InsuranceId.valueStr() && !BatchNumber.valueStr())
            {
                element.clearRanges();
        
                q = HMClaimChargeLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMClaimChargeLine));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::OuterJoin);
        
                // Musab
                q = HMCaseInsurance_ds.query();
                qbds = q.dataSourceTable(tablenum(HMCaseInsurance));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::InnerJoin);
        
                qbdsSearchField = q.dataSourceTable(tablenum(HMCaseInsurance));
        
                qbr = qbdsSearchField.addRange(fieldnum(HMCaseInsurance, InsuranceCarrier));
                qbr.value(InsuranceId.valueStr());
                qr = new QueryRun(q);
        
                HMPatientChargeEntryTable_ds.query(qr.query());
        
                HMPatientChargeEntryTable_ds.executeQuery();
        
            }
            if (HMChargeEntryFilter.valueStr() == "Reversed")
            {
        
                q = HMClaimChargeLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMClaimChargeLine));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::OuterJoin);
        
                if (!InsuranceId.valueStr() && !BatchNumber.valueStr())
                {
                    // Musab
                    q = HMCaseInsurance_ds.query();
                    qbds = q.dataSourceTable(tablenum(HMCaseInsurance));
                    qbds.clearRanges();
                    qbds.joinMode(JoinMode::OuterJoin);
                }
        
                q = HMCaseTrans_ds.query();
                qbds = q.dataSourceTable(tablenum(HMCaseTrans));
                qbds.clearRanges();
        
                qbdsSearchField = q.dataSourceTable(tablenum(HMCaseTrans));
        
                qbr = qbdsSearchField.addRange(fieldnum(HMCaseTrans, BillingStatus));
                qbr.value("Reveresed");
                qr = new QueryRun(q);
        
                HMPatientChargeEntryTable_ds.query(qr.query());
        
                HMPatientChargeEntryTable_ds.executeQuery();
            }
            if (HMChargeEntryFilter.valueStr() == "No Batches")
            {
                q = HMClaimChargeLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMClaimChargeLine));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::OuterJoin);
        
                if (!InsuranceId.valueStr() && !BatchNumber.valueStr())
                {
                    // Musab
                    q = HMCaseInsurance_ds.query();
                    qbds = q.dataSourceTable(tablenum(HMCaseInsurance));
                    qbds.clearRanges();
                    qbds.joinMode(JoinMode::OuterJoin);
                }
        
                q = HMPatientChargeEntryLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMPatientChargeEntryLine));
                qbds.clearRanges();
        
                qbdsSearchField = q.dataSourceTable(tablenum(HMPatientChargeEntryLine));
        
                qbr = qbdsSearchField.addRange(fieldnum(HMPatientChargeEntryLine, HMInsuranceCarrierBatchDetails));
                qbr.value('0');
                qr = new QueryRun(q);
        
                HMPatientChargeEntryTable_ds.query(qr.query());
        
                HMPatientChargeEntryTable_ds.executeQuery();
        
            }
            if (HMChargeEntryFilter.valueStr() == "Claim")
            {
        
                if (!InsuranceId.valueStr() && !BatchNumber.valueStr())
                {
                    // Musab
                    q = HMCaseInsurance_ds.query();
                    qbds = q.dataSourceTable(tablenum(HMCaseInsurance));
                    qbds.clearRanges();
                    qbds.joinMode(JoinMode::OuterJoin);
                }
        
                q = HMClaimChargeLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMClaimChargeLine));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::InnerJoin);
        
                qbdsSearchField = q.dataSourceTable(tablenum(HMClaimChargeLine));
        
                qr = new QueryRun(q);
        
                HMPatientChargeEntryTable_ds.query(qr.query());
        
                HMPatientChargeEntryTable_ds.executeQuery();
        
            }
            if (HMChargeEntryFilter.valueStr() == "Not Claimed")
            {
        
                if (!InsuranceId.valueStr() && !BatchNumber.valueStr())
                {
                    // Musab
                    q = HMCaseInsurance_ds.query();
                    qbds = q.dataSourceTable(tablenum(HMCaseInsurance));
                    qbds.clearRanges();
                    qbds.joinMode(JoinMode::OuterJoin);
                }
        
                q = HMClaimChargeLine_ds.query();
                qbds = q.dataSourceTable(tablenum(HMClaimChargeLine));
                qbds.clearRanges();
                qbds.joinMode(JoinMode::NoExistsJoin);
        
                qbdsSearchField = q.dataSourceTable(tablenum(HMClaimChargeLine));
        
                qr = new QueryRun(q);
        
                HMPatientChargeEntryTable_ds.query(qr.query());
        
                HMPatientChargeEntryTable_ds.executeQuery();
        
            }
        
        
        }

]]></Source>
