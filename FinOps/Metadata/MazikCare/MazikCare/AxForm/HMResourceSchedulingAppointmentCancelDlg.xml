        public void init()
        {
            HMResourceSchedulingAppointment appt;
        
            appt = element.args().record();
        
            super();

            HMResourceSchedulingAppointment_ds.markAllLoadedRecords(true);
        
            CancellationDeadLineDate.dateValue(cancellationDate);
            if(cancellationDate && cancellationDate+1 <= DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone()))
            {
                DeadlineReached.selection(1);
            }
        
            this.queryBuildDataSource().clearDynalinks();
        
            this.queryBuildDataSource().addRange(fieldNum(HMResourceSchedulingAppointment,HMPatientVisit)).value(SysQuery::value(appt.HMPatientVisit));
        
            this.queryBuildDataSource().addRange(fieldNum(HMResourceSchedulingAppointment,AppointmentStatus)).value(SysQuery::value(HMResourceSchedulingAppointmentStatus::Confirmed));
            this.queryBuildDataSource().addRange(fieldNum(HMResourceSchedulingAppointment,AppointmentStatus)).value(SysQuery::value(HMResourceSchedulingAppointmentStatus::Reserved));
            this.queryBuildDataSource().addRange(fieldNum(HMResourceSchedulingAppointment,AppointmentStatus)).value(SysQuery::value(HMResourceSchedulingAppointmentStatus::ToBeReschedule));
        }

]]></Source>
