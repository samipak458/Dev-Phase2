			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        #Task
        HMResourceSchedulingAppointment HMResourceSchedulingAppointmentLocal;
        List                            apptRecIdList = new List(Types::Int64);
        SetEnumerator                   setEnumerator;
        Set                             apptRecIdSet = new Set(Types::Int64);
        RecId                           patientRecid;
        date                            apptDate;
        Object                          formRun;
        FormRun                         caller;
        DialogButton                    dialogButton;
        boolean                         cancelAll = false;
        
        HMResourceSchedulingAppointmentLocal = HMResourceSchedulingAppointment_ds.getFirst();
        
        if (HMAppointLogNote.text() == '')
        {
            error("Cancellation requires the notes field to be filled.");
        }
        else
        {
            dialogButton = Box::yesNo("@MAZ11916", DialogButton::No, "@SYS301622");
            if (dialogButton == DialogButton::Yes)
            {
                while(HMResourceSchedulingAppointmentLocal)
                {
                    apptRecIdSet.add(HMResourceSchedulingAppointmentLocal.RecId);
                    if(HMResourceSchedulingAppointmentLocal.PrimaryAppointment == YesNo::Yes)
                    {
                        cancelAll = true;
                    }
                    apptDate = HMResourceSchedulingAppointmentLocal.AppointmentDate;
                    patientRecid = HMResourceSchedulingAppointmentLocal.HMPatient;
                    HMResourceSchedulingAppointmentLocal = HMResourceSchedulingAppointment_ds.getNext();
                }
                if(cancelAll)
                {
                    HMResourceSchedulingAppointmentLocal = HMResourceSchedulingAppointment_ds.getFirst(false);
                    while(HMResourceSchedulingAppointmentLocal)
                    {
                        apptRecIdSet.add(HMResourceSchedulingAppointmentLocal.RecId);
                        apptDate = HMResourceSchedulingAppointmentLocal.AppointmentDate;
                        patientRecid = HMResourceSchedulingAppointmentLocal.HMPatient;
                        HMResourceSchedulingAppointmentLocal = HMResourceSchedulingAppointment_ds.getNext();
                    }
                }
                setEnumerator = apptRecIdSet.getEnumerator();
                while(setEnumerator.moveNext())
                {
                    apptRecIdList.addEnd(setEnumerator.current());
                }
    
                if(!apptRecIdList.empty())
                {
                    if(HMResourceSchedulingHelper::cancelAppointment(apptRecIdList,CancelSubStatus.selection(),HMAppointLogNote.text(),
                                                                    HMAppointLogDate.dateValue(),HMAppointLogTime.value(),patientRecid,apptDate))
                    {
                        super();
                        formRun = element.args().caller();
                        caller = element.args().caller();
                        if(caller.name() == formStr(Hmappointmentsearch))
                        {
                            if (formHasMethod(formRun, identifierstr(refreshGrid)))
                            {
                                element.args().caller().refreshGrid('','','');
                            }
                            else
                            {
                                formRun.task(#taskRefresh);
                            }
                        }
                        else
                        {
                            if (formHasMethod(formRun, identifierstr(refreshGrid)))
                            {
                                element.args().caller().refreshGrid();
                            }
                            else
                            {
                                formRun.task(#taskRefresh);
                            }
                        }
                    }
    
                }
            }
        }
    }

]]></Source>
