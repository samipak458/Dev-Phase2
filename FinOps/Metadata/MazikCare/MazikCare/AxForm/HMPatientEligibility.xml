				<Methods>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            SysTableLookup sysTableLookup = SysTableLookup::newParameters(tablenum(HMPatientInsurance),this,true);
        
            Query query;
            QueryBuildDataSource queryBuildDataSource;
        
            sysTableLookup.addLookupfield(fieldnum(HMPatientInsurance,HMPatientInsuranceMembershipId),true);
            sysTableLookup.addLookupfield(fieldnum(HMPatientInsurance,InsuranceCarrier));
            sysTableLookup.addLookupMethod(tableMethodStr(HMPatientInsurance, carrierName));
        
            query = new Query();
        
            queryBuildDataSource = query.addDataSource(tablenum(HMPatientInsurance));
        
            queryBuildDataSource.addRange(fieldnum(HMPatientInsurance,HMPatient)).value(queryvalue(patientRecId));
        
            sysTableLookup.parmQuery(query);
            sysTableLookup.parmUseLookupValue(true);
        
            sysTableLookup.performFormLookup();
        }

]]></Source>
					</Method>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret;
            HMPatientInsurance      hmPatientInsurance;
        
            ret = super();
        
            if(ret)
            {
                if (this.valueStr() != "")
                {
                    hmPatientInsurance = HMlookupClass::checkValidPatientInsurance(patientRecId, this.valueStr());
        
                    if (hmPatientInsurance.RecId)
                    {
                        patientInsuranceRecId = hmPatientInsurance.RecId;
                        InsuranceProviderId.text(HMInsuranceCarrier::findByCode(hmPatientInsurance.InsuranceCarrier).HMProviderID);
                    }
        
                    else
                    {
                        warning(strFmt("@SYS316757","@MAZ1254"));
                        this.text(HMPatientInsurance::find(patientInsuranceRecId).HMPatientInsuranceMembershipId);
                    }
                }
                else
                {
                    patientInsuranceRecId = 0;
                    InsuranceProviderId.text('');
                }
            }
        
            return ret;
        }

]]></Source>
					</Method>
