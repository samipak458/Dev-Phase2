			<Control>
				<Name>BypassEligibiltiy</Name>
				<Type>String</Type>
				<Methods>
					<Method>
						<Name>jumpRef</Name>
						<Source><![CDATA[
        public void jumpRef()
        {
            Args                             args;
            MenuFunction                     menuFunction;
        
            args = new Args();
            args.caller(element);
        
            menuFunction = new MenuFunction(menuitemdisplaystr(HMEligibilityBypassReason),MenuItemType::Display);
        
            menuFunction.run(args);
        }

]]></Source>
					</Method>
					<Method>
						<Name>lookup</Name>
						<Source><![CDATA[
        public void lookup()
        {
            Args args = new Args();
            FormRun itemLookUp;
        
            args.name(formstr(HMReasonsLookup));
            args.caller(element);
            args.parm('EligibilityBypass');
            itemLookUp = ClassFactory.formRunClass(args);
            itemLookUp.init();
            this.performFormLookup(itemLookUp);
        }

]]></Source>
					</Method>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            ReasonTable             reasonTable;
            boolean                 ret;
        
            ret = super();
        
            if (ret)
            {
                if (BypassEligibiltiy.valueStr() != "")
                {
                    select * from reasonTable
                        where
                        reasonTable.Description == BypassEligibiltiy.valueStr()
                        &&  reasonTable.EligibilityBypass == NoYes::Yes;
        
                        if (reasonTable.RecId)
                        {
                            //flag = true;
                            bypassReasonRecId = reasonTable.RecId;
                        }
        
                        else
                        {
                           
                            warning(strFmt("@SYS316757","@MAZ12991"));
                        //todo: ax7 reason
                        /*
                            BypassEligibiltiy.text(ReasonTable::findByRecid(bypassReasonRecId).Description);
                        */
                        
                        }
                }
                else
                {
                    bypassReasonRecId = 0;
        
                }
        
            }
            return ret;
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>OKButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            if(element.validate())
            {
                hmPatientEligibilityClass.createEligibilityLog(patientRecId, patientInsuranceRecId,MembershipId.valueStr(),
                        Eligible.selection(), bypassReasonRecId);
        
                super();
            }
        }

]]></Source>
