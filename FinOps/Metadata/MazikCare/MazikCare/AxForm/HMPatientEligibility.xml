			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        hmPatientEligibilityClass = new HMPatientEligibility();
        patientRecId = element.args().record().RecId;
    
        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>storeReason</Name>
				<Source><![CDATA[
    public void storeReason(ReasonTable  _reasonTable)
    {
        bypassReasonRecId = _reasonTable.RecId;
        BypassEligibiltiy.text(_reasonTable.Description);
    }

]]></Source>
			</Method>
			<Method>
				<Name>validate</Name>
				<Source><![CDATA[
    public boolean validate()
    {
        boolean ret = true;
    
        if(!MembershipId.valueStr())
        {
            warning(strFmt("@SYS84753","@MAZ12985"));
            ret = false;
        }
    
        return ret;
    }

]]></Source>
