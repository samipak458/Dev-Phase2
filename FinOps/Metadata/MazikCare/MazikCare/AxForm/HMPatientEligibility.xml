						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            super();
        
            if(!BypassEligibiltiy.valueStr())
            {
                warning(strFmt("@SYS84753","@MAZ12991"));
            }
            else
            {
                Eligible.selection(NoYes::Yes);
            }
        }

]]></Source>
