			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        HMCaseTrans                 hmCaseTrans, hmCaseTransNew;
        FormRun caller;
        Common common;
        HMResourceSchedulingAppointment hmResourceSchedulingAppoinment;
    
        caller = element.args().caller();
        common = element.args().record();
    
        if (caller.args().menuItemName() == menuitemDisplayStr(HMPatientChargeEntryAdvancePaymentPharm))
        {
            HMPharmacyClinicDataHelper::delPastMedications();
    
            hmResourceSchedulingAppoinment = common;
            if (hmResourceSchedulingAppoinment)
            {
                hmPatientPastMedicationsLocal = HMPharmacyClinicDataHelper::setPatientPastMedication(HMPharmacyClinicDataHelper::getGuid(int642str(hmResourceSchedulingAppoinment.HMPatient)));
            }
        }
        HMPatientPastMedications.setTmpData(hmPatientPastMedicationsLocal);
        super();
    }

]]></Source>
