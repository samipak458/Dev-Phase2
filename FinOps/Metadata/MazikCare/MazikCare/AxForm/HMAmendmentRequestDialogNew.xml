				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            #task
            Object  formRunObject = formRun;
        
            if (HMAmendmentRequest.RecId == 0)
            {
                HMAmendmentRequest_ds.write();
            }
        
            super();
            if (formRun.name() == formStr(HMAmendmentRequestsListPage))
            {
                formRun.task(#taskRefresh);
            }
            else
            {
                if (formHasMethod(formRun, identifierstr(refreshAmendmendsGrid)))
                {
                    formRunObject.refreshAmendmendsGrid();
                }
            }
        }

]]></Source>
