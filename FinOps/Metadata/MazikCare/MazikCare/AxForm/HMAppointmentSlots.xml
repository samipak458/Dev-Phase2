			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        HMWaitingList   hmWaitingList;
        HMCaseTrans     hmCaseTrans;
        HMReferralsTmp  hmReferralsTmp;
    
        FromDate.dateValue(DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone()));
        ToDate.dateValue(DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone()));
    
        if(element.args().record())
        {
            if(element.args().record() is HMWaitingList)
            {
                hmWaitingList = element.args().record();
    
                FromDate.dateValue(hmWaitingList.ExpectedDate);
                ToDate.dateValue(hmWaitingList.ExpectedDate);
    
                scheduleTypeRecId = hmWaitingList.HMResourceSchedulingTypeCode;
                //resourceScheduleRecId = hmWaitingList.HMResourceSchedulingPlanningObject;
                patientRecId = hmWaitingList.HMPatient;
    
                //Resource.text(HMResourceSchedulingPlanningObject::find(resourceScheduleRecId).Description);
                ScheduleType.text(HMResourceSchedulingTypeCode::find(scheduleTypeRecId).Description);
    
                callerMenuItemCaller = element.args().menuItemName();
    
                Search.clicked();
            }
            else if(element.args().record() is HMCaseTrans)
            {
                hmCaseTrans = element.args().record();
    
                patientRecId = hmCaseTrans.HMPatient;
    
                callerMenuItemCaller = element.args().menuItemName();
            }
            else if(element.args().record() is HMReferralsTmp)
            {
                hmReferralsTmp = element.args().record();
    
                patientRecId = hmReferralsTmp.HMPatient;
                resourceRecId = hmReferralsTmp.HMResourceMaster;
    
                Resource.text(HMResourceMaster::find(resourceRecId).Description);
    
                callerMenuItemCaller = element.args().menuItemName();
    
                if(resourceRecId)
                {
                    Search.clicked();
                }
            }
        }
    
        super();
    
        showInfoOnNoSLots = true;
    }

]]></Source>
