				<Source><![CDATA[
[Form]
public class HMLogisticsPostalAddressLookup extends FormRun
{
    Common entity;
    container roleRecIds;
    boolean returnLocation;
    container inUseAddresses;

    #define.LogisticsLocationMain('LogisticsLocationMain')
    #define.LogisticsPostalAddressMain('LogisticsPostalAddressMain')

    #define.PartyAddressesTab(1)
    #define.TransactionAddressesTab(2)

    str         callerMethod;
}
]]></Source>
			</Method>
			<Method>
				<Name>closeSelect</Name>
				<Source><![CDATA[
    public void closeSelect(str _selectString)
    {
        //LogisticsPostalAddressView  LogisticsPostalAddressView;
    
        if(callerMethod == 'storeLogisticPostalAddress')
        {
            element.args().caller().storeLogisticPostalAddress(LogisticsPostalAddressMain.RecId);
        }
        else if(callerMethod == 'storeReportPostalAddress')
        {
            element.args().caller().storeReportPostalAddress(LogisticsPostalAddressMain.RecId);
        }
        //select Address from LogisticsPostalAddressView
        //where LogisticsPostalAddressView._RecId_LogisticsPostalAddress == LogisticsPostalAddressMain.RecId;
        super(HMResourceSchedulingHelper::getAddressDescription(LogisticsPostalAddressMain.RecId));
    }

]]></Source>
			</Method>
			<Method>
				<Name>entityRecId</Name>
				<Source><![CDATA[
    public RecId entityRecId()
    {
        DirParty party;
    
        if (entity.TableId == tableNum(InventLocation) || entity.TableId == tableNum(InventSite))
        {
            return entity.RecId;
        }
        else
        {
            // Entity is a party
            party = new DirParty(entity);
            return party.getPartyRecId();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>executeQuery</Name>
				<Source><![CDATA[
    public void executeQuery()
    {
        if (returnLocation)
        {
            logisticsLocationMain_ds.executeQuery();
        }
        else
        {
            logisticsPostalAddressMain_ds.executeQuery();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>getFormBuildDataSource</Name>
				<Source><![CDATA[
    private FormBuildDataSource getFormBuildDataSource(str _datasourceName)
    {
        FormBuildDataSource fbds;
        int i;
    
        for (i = 1; i <= this.form().dataSourceCount(); i++)
        {
            fbds = this.form().dataSource(i);
    
            if (fbds.name() == _datasourceName)
            {
                return fbds;
            }
        }
    
        return null;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    #Define.TrueStr('true')
    public void init()
    {
        str roleTypesLocal;
        str inUseAddressLocal;
        str returnLocationLocal;
        str parm = element.args().parm();
        container roleTypes;
        FormBuildDataSource fbds;
        int i;
    
        // Get setup data
        entity = element.args().record();
        [roleTypesLocal, returnLocationLocal, inUseAddressLocal, callerMethod] = str2con(parm, ";");
        roleTypes = str2con(roleTypesLocal);
        if (inUseAddressLocal != '')
        {
            inUseAddresses = str2con(inUseAddressLocal);
        }
        else
        {
            inUseAddresses = conNull();
        }
        returnLocation = returnLocationLocal == #TrueStr ? true : false;
    
        // Convert roleTypes to RecIds
        roleRecIds = conNull();
        for (i = 1; i <= conLen(roleTypes); i++)
        {
            roleRecIds += LogisticsLocationRole::findBytype(conPeek(roleTypes, i)).RecId;
        }
    
        if (returnLocation)
        {
            fbds = this.getFormBuildDataSource(#LogisticsPostalAddressMain);
            fbds.joinSource(this.getFormBuildDataSource(#LogisticsLocationMain).id());
            fbds.linkType(FormLinkType::InnerJoin);
        }
        else
        {
            fbds = this.getFormBuildDataSource(#LogisticsLocationMain);
            fbds.joinSource(this.getFormBuildDataSource(#LogisticsPostalAddressMain).id());
            fbds.linkType(FormLinkType::InnerJoin);
        }
    
        super();
    
        if (conLen(inUseAddresses) <= 0)
        {
            // Hide extra tab
            tab.showTabs(false);
        }
    
        if (returnLocation)
        {
            gridEntity.dataSource(logisticsLocationMain_ds.name());
            gridInUse.dataSource(logisticsLocationMain_ds.name());
        }
        else
        {
            gridEntity.dataSource(logisticsPostalAddressMain_ds.name());
            gridInUse.dataSource(logisticsPostalAddressMain_ds.name());
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateQuery</Name>
				<Source><![CDATA[
    public void updateQuery()
    {
        QueryBuildDataSource qbds;
        QueryBuildRange qbr;
        
        TableId entityLocationTableId;
        TableId viewTableId;
    
        FieldId viewEntityFieldId ;
        FieldId viewLocationFieldId;
        FieldId viewLocationRoleFieldId;

        //todo: ax7 method not exsits ... after resolving issue deleter above tableid and field id
        /*
        TableId entityLocationTableId = LogisticsEntityLocationMap::getEntityLocationTableId();
        TableId viewTableId = LogisticsEntityLocationMap::getEntityPrimaryAndRoleViewTableId(entityLocationTableId);
    
        FieldId viewEntityFieldId = LogisticsEntityLocationMap::getEntityPrimaryAndRoleViewEntityFieldId(viewTableId);
        FieldId viewLocationFieldId = LogisticsEntityLocationMap::getEntityPrimaryAndRoleViLocationFieldId(viewTableId);
        FieldId viewLocationRoleFieldId = LogisticsEntityLocationMap::getEntityPrimaryViewLocationRoleFieldId(viewTableId);
        */
        // Use DirPartyLocationPrimaryAndRolesView (or similar for InventSite/InventLocation) to filter on primary and roles
        qbds = SysQuery::findOrCreateDataSource(logisticsLocationMain_ds.query(), viewTableId, tableNum(LogisticsLocation));
        qbds.joinMode(JoinMode::ExistsJoin);
        qbds.clearLinks();
        qbds.addLink(fieldNum(LogisticsLocation, RecId), viewLocationFieldId);
    
    
        if (tab.tab() == #TransactionAddressesTab)
        {
            qbds.enabled(false);
    
            qbr = SysQuery::findOrCreateRange(logisticsPostalAddressMain_ds.queryBuildDataSource(),
                fieldNum(LogisticsPostalAddress, RecId));
            qbr.value(con2StrUnlimited(inUseAddresses));
        }
        else
        {
            // Clear the filter
            logisticsPostalAddressMain_ds.queryBuildDataSource().clearRanges();
            LogisticsLocationMain_ds.queryBuildDataSource().clearDynalinks();
            qbds.enabled(true);
        }
    
        if (tab.tab() == #PartyAddressesTab)
        {
            // Add range on party
            qbr = SysQuery::findOrCreateRange(qbds, viewEntityFieldId);
            qbr.value(SysQuery::value(element.entityRecId()));
    
            // Add range for primary or roles
            // In DirPartyLocationPrimaryAndRolesView, locationRole = 0 indicates a primary address
            qbr = SysQuery::findOrCreateRange(qbds, viewLocationRoleFieldId);
            if (conLen(roleRecIds) > 0)
            {
                qbr.value(strFmt("%1", con2StrUnlimited(roleRecIds)));
            }
            else
            {
                qbr.value(SysQuery::value(0));
            }
        }
        else
        {
            // Clear ranges
            qbds.clearRanges();
        }
    }

]]></Source>
