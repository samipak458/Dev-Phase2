			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
    
    
        patientRecid = element.args().record().RecId;
        dateTime = DateTimeUtil::applyTimeZoneOffset(DateTimeUtil::getSystemDateTime(),DateTimeUtil::getCompanyTimeZone());
        UtcDateTimeEdit.dateTimeValue(dateTime);
    
    }

]]></Source>
