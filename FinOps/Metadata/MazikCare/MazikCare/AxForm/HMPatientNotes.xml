		<DataControls xmlns="">
			<Control>
				<Name>Close</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            Object                                  formRunObject;
            FormRun                                  formRun;
            formRun = element.args().caller();
        
            super();
            if(NotesEdit.text() != '')
            {
                HMPatientNotes.Notes                    = NotesEdit.text();
                HMPatientNotes.DateTime                 = dateTime;               //DateTimeUtil::getSystemDateTime();
                HMPatientNotes.HMPatient                = patientRecid;
                HMPatientNotes.HMPatientChargeEntryTable = patientChargeEntryRecid;
                HMPatientNotes.insert();
        
                if (element.args().parm() == 'ChargeEntry')
                {
                    if (formHasMethod(formRun,identifierstr(bindPatientNotesRefresh)))
                    {
                        formRunObject = formRun;
                        formRunObject.bindPatientNotesRefresh();
                    }
                }
                else
                {
                    if (formHasMethod(formRun,identifierstr(bindPatientNotes)))
                    {
                        formRunObject = formRun;
                        formRunObject.bindPatientNotes();
                    }
        
                }
        
            }
             element.close();
        }

]]></Source>
					</Method>
