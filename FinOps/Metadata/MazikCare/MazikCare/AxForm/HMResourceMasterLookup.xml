				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            if(element.args().parmEnum() == HMResourceType::None)
            {
                if(element.args().parm() == 'NoDeviceNoExternal')
                {
                    this.queryBuildDataSource().addRange(fieldNum(HMResourceMaster, Type)).value(SysQuery::valueNot(HMResourceType::Device));
                    this.queryBuildDataSource().addRange(fieldNum(HMResourceMaster, VendorType)).value(SysQuery::value(HMVendorType::Internal));
                }
                else if(element.args().parm() == 'NoDevice')
                {
                    this.queryBuildDataSource().addRange(fieldNum(HMResourceMaster, Type)).value(SysQuery::valueNot(HMResourceType::Device));
                }
                else if(element.args().parm() == 'FilterOnOU')
                {
                    this.queryBuildDataSource().addDataSource(tableNum(HMResourceOrganizationalUnit)).relations(true);
                    this.query().dataSourceTable(tableNum(HMResourceOrganizationalUnit)).addRange(fieldNum(HMResourceOrganizationalUnit,HMOrganizationalUnit)).value(SysQuery::value(element.args().record().RecId));
                }
            }
            if(element.args().parmEnum())
            {
                this.queryBuildDataSource().addRange(fieldNum(HMResourceMaster, Type)).value(SysQuery::value(element.args().parmEnum()));
            }
            this.queryBuildDataSource().addRange(fieldNum(HMResourceMaster, Type)).value(SysQuery::range(HMResourceType::Device, HMResourceType::Employee));
            this.queryBuildDataSource().addRange(fieldNum(HMResourceMaster, Status)).value(SysQuery::value(HMResourceStatus::Active));
        }

]]></Source>
