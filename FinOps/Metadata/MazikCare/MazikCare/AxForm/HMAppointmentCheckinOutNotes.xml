			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        HMResourceSchedulingAppointment appt;
        HMAppoinmentLog                 apptLog;
        super();
        appt = element.args().record();
        SupplementalInstruction.text(strFmt('%1 - %2',appt.getPatientName(),appt.AppointmentNumber));
    
        select firstOnly HMAppointLogNote from apptLog
        order by CreatedDateTime desc
        where apptLog.HMResScheduleAppointment == appt.RecId
        && apptLog.HMAppointLogStatus == HMResourceSchedulingAppointmentStatus::CheckedIn;
    
        CheckinNotes.text(apptLog.HMAppointLogNote);
    
        select firstOnly HMAppointLogNote from apptLog
        order by CreatedDateTime desc
        where apptLog.HMResScheduleAppointment == appt.RecId
        && apptLog.HMAppointLogStatus == HMResourceSchedulingAppointmentStatus::CheckedOut;
    
        CheckoutNotes.text(apptLog.HMAppointLogNote);
    
    }

]]></Source>
