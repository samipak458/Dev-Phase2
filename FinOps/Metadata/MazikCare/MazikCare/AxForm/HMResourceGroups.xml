				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class HMResourceGroups extends FormRun
{
    int minimumPriority,maximumPriority;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        //Initialize splitter
    }

]]></Source>
			</Method>
			<Method>
				<Name>initPriorities</Name>
				<Source><![CDATA[
    public void initPriorities()
    {
        int totalResources = HMResourceGroupResourceMaster::getMinPriorityByResourceGroup(HMResourceGroupResourceMaster.HMResourceGroup);
    
        if (totalResources > 1)
        {
            minimumPriority = totalResources;
            maximumPriority = 1;
        }
        else if (totalResources == 1)
        {
            minimumPriority = 1;
            maximumPriority = 1;
        }
        else
        {
            minimumPriority = 0;
            maximumPriority = 1;
        }
    
        element.setButtonsAccess();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setButtonsAccess</Name>
				<Source><![CDATA[
    public void setButtonsAccess()
    {
        // if lowest and highest priorities are equal.
        if (minimumPriority == maximumPriority)
        {
            bttnMoveUp.enabled(false);
            bttnMoveDown.enabled(false);
        }
        // if selected billing code priority is higest.
        else if (HMResourceGroupResourceMaster.Priority == maximumPriority)
        {
            bttnMoveUp.enabled(false);
            bttnMoveDown.enabled(true);
        }
        // if selected billing code priority is lowest.
        else if (HMResourceGroupResourceMaster.Priority == minimumPriority)
        {
            bttnMoveUp.enabled(true);
            bttnMoveDown.enabled(false);
        }
        else
        {
            bttnMoveUp.enabled(true);
            bttnMoveDown.enabled(true);
        }
        if (!HMResourceGroupResourceMaster.RecId)
        {
            bttnMoveUp.enabled(false);
            bttnMoveDown.enabled(false);
        }
    }

]]></Source>
			</Method>
		</Methods>
