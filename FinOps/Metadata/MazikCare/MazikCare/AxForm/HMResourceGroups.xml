			<DataSource>
				<Name>HMResourceGroupResourceMaster</Name>
				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            this.query().dataSourceTable(tablenum(HMResourceGroupResourceMaster)).addSortField(fieldNum(HMResourceGroupResourceMaster,Priority),SortOrder::Ascending);
        }

]]></Source>
					</Method>
					<Method>
						<Name>write</Name>
						<Source><![CDATA[
        public void write()
        {
            RecId selectedResourceRecId;
            boolean isRecordInserted = false;
            if(HMResourceGroupResourceMaster.RecId)
            {
                isRecordInserted = true;
            }
        
            super();
        
        
            if(!isRecordInserted)
            {
                minimumPriority = HMResourceGroupResourceMaster.Priority;
            }
            selectedResourceRecId = HMResourceGroupResourceMaster.RecId;
            HMResourceGroupResourceMaster_ds.executeQuery();
            HMResourceGroupResourceMaster_ds.positionToRecord(HMResourceGroupResourceMaster::find(selectedResourceRecId));
        }

]]></Source>
					</Method>
					<Method>
						<Name>delete</Name>
						<Source><![CDATA[
        public void delete()
        {
            int  priority;
        
            priority = HMResourceGroupResourceMaster.Priority;
            super();
            if (priority)
            {
                minimumPriority -= 1;
                this.research();
                if (maximumPriority != priority)
                {
                    this.positionToRecordByValue(fieldNum(HMResourceGroupResourceMaster, Priority), int2str(priority));
                }
                element.initPriorities();
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
            element.initPriorities();
            return ret;
        }

]]></Source>
					</Method>
