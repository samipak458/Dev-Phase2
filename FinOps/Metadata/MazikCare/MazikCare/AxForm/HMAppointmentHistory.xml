        public int active()
        {
            int ret;
        
            ret = super();
        
            HMAppointmentCalendarApptHistory.enabled(HMResourceSchedulingAppointment.AppointmentStatus != HMResourceSchedulingAppointmentStatus::Cancelled && HMResourceSchedulingAppointment.AppointmentStatus != HMResourceSchedulingAppointmentStatus::ToBeReschedule);
        
            return ret;
        }

]]></Source>
