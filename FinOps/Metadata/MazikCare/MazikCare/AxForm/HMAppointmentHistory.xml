						<Source><![CDATA[
        public void init()
        {
            QueryBuildDataSource  qbds;
            super();
            this.queryBuildDataSource().addRange(fieldNum(HMResourceSchedulingAppointment,HMPatient)).value(SysQuery::value(element.args().record().RecId));
            this.queryBuildDataSource().addSortField(fieldNum(HMResourceSchedulingAppointment,AppointmentDate),SortOrder::Descending);
            this.queryBuildDataSource().addSortField(fieldNum(HMResourceSchedulingAppointment,AppointmentTime),SortOrder::Descending);
        }

