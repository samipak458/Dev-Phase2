                this.queryBuildDataSource().addGroupByField(fieldNum(HMResourceGroup,Code));
                this.queryBuildDataSource().addGroupByField(fieldNum(HMResourceGroup,Description));
                this.queryBuildDataSource().addGroupByField(fieldNum(HMResourceGroup,RecId));
            }
        }

