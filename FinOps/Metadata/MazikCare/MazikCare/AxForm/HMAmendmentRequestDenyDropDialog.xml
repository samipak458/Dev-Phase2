				<Name>OKButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            FormRun formRun         = element.args().caller();
            Object  formRunObject   = formRun;
        
        
            if (HMAmendmentRequest.DenialReason == "")
            {
                checkFailed("@MAZ6614");
            }
            else
            {
                super();
                if (formHasMethod(formRun, identifierstr(refreshAmendmendsGrid)))
                {
                    formRunObject.refreshAmendmendsGrid();
                }
            }
        }

]]></Source>
