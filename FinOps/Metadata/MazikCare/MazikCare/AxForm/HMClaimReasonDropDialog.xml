				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            #Task
            HMClaimReasonTable                  hmClaimReasonTable;
            FormRun                             formRun;
            ReasonTable                         reasonTableLocal;
            HMClaimTableGeneration              hmClaimTableGeneration;
            RecId                               claimVerRecId;
            boolean                             noRecordSelected = true;
        
        
            if(ReasonTable_ds.recordsMarked().lastIndex() == 0)
            {
                info("@MAZ7568");
                return;
            }
        
            formRun                 = element.args().caller();
            hmClaimTableGeneration  = new HMClaimTableGeneration();
            reasonTableLocal        = ReasonTable_ds.getFirst(true);
        
            isokclicked = true;
            HMClaim_ds.write();
           
            if(element.args().menuItemName() == menuitemDisplayStr(HMClaimReasonDropDialogDenial))
            {
                claimVerRecId = hmClaimTableGeneration.updateClaimStatus(FormDataUtil::getFormDataSource(element.args().record()),HMClaimStatus::Denied);
        
            }
            else if(element.args().menuItemName() == menuitemDisplayStr(HMClaimReasonDropDialogRejection))
            {
                claimVerRecId = hmClaimTableGeneration.updateClaimStatus(FormDataUtil::getFormDataSource(element.args().record()),HMClaimStatus::Rejected);
            }
       
            while(reasonTableLocal)
            {
                noRecordSelected = false;
                hmClaimReasonTable.initValue();
                hmClaimReasonTable.HMClaimVersion = claimVerRecId;
                hmClaimReasonTable.ReasonTable = reasonTableLocal.Reason;
                hmClaimReasonTable.insert();
                reasonTableLocal = ReasonTable_ds.getNext();
            }
        
        
        
            if(element.args().parm() == 'RefreshQuery')
            {
                element.args().caller().task(#taskrefresh);
            }
        
            super();
        }

]]></Source>
