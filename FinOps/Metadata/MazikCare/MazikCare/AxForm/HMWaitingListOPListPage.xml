				<Methods>
					<Method>
						<Name>displayOption</Name>
						<Source><![CDATA[
        public void displayOption(Common _record, FormRowDisplayOption _options)
        {
            HMWaitingList HMWaitingListLocal =  HMWaitingList::find(_record.RecId);
            int colourCode;
        
            if (HMWaitingListLocal.RecId)
            {
                if (HMPatient::find(HMWaitingListLocal.HMPatient).IsVIP)
                {
                    _options.backColor(HMColourCodingParameters::findByElementNameAndValue(extendedTypeStr(HMVIP),enum2str(NoYes::Yes)).ColourCode);
                }
                else
                {
                    colourCode = HMColourCodingParameters::findByElementNameAndValue(enumStr(HMPriority),enum2str(HMWaitingListLocal.Priority)).ColourCode;
                    if (colourCode)
                    {
                        _options.backColor(colourCode);
                        _options.affectedElementsByControl(PriorityColour.id());
                    }
                }
            }
        
            super(_record, _options);
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            this.query().dataSourceTable(tableNum(HMWaitingList)).addRange(fieldNum(HMWaitingList,WaitType)).value(SysQuery::value(HMWaitType::OP));
            this.query().dataSourceTable(tableNum(HMWaitingList)).addRange(fieldNum(HMWaitingList,WaitType)).value(SysQuery::value(HMWaitType::Emergency));
            this.query().dataSourceTable(tableNum(HMWaitingList)).addRange(fieldNum(HMWaitingList,WaitType)).value(SysQuery::value(HMWaitType::Physiotherapy));
            this.query().dataSourceTable(tableNum(HMWaitingList)).addRange(fieldNum(HMWaitingList,Status)).value(SysQuery::valueNot(HMWaitingListStatus::Cancelled));
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            if (ret)
            {
                if (HMWaitingList_1.Status == HMWaitingListStatus::Open)
                {
                    HMResourceSchedulingAppointmentWaiting.enabled(true);
                    HMWaitingListCancel.enabled(true);
                }
                else
                {
                    HMResourceSchedulingAppointmentWaiting.enabled(false);
                    HMWaitingListCancel.enabled(false);
                }
            }
        
            return ret;
        }

]]></Source>
