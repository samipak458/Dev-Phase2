        void clicked()
        {
            HMResourceSchedulingOpenTimeSlotsTmp  HMResourceSchedulingOpenTimeSlotsTmpLocal;
            str parm;
        
            Args        args = new Args();
             HMResourceSchedulingOpenTimeSlotsTmpLocal = HMResourceSchedulingOpenTimeSlotsTmp_1_ds.getFirst(true);
            if (!HMResourceSchedulingOpenTimeSlotsTmpLocal)
            {
                HMResourceSchedulingOpenTimeSlotsTmpLocal = HMResourceSchedulingOpenTimeSlotsTmp_2_ds.getFirst(true);
            }
            if (!HMResourceSchedulingOpenTimeSlotsTmpLocal)
            {
                error(SysLabel::labelId2String2('@MAZ11059'));
                return;
            }
            parm = strFmt('%1',HMResourceSchedulingOpenTimeSlotsTmpLocal.Date) + ',' + strFmt('%1',(time2StrHMLeadingZero(HMResourceSchedulingOpenTimeSlotsTmpLocal.Time))) +','+ int642str(HMResourceSchedulingOpenTimeSlotsTmpLocal.HMResourceSchedulingTypeCode) +','+ int642str(element.args().record().RecId) ;
        
        
        
        
            args.parm(parm);
            args.record(HMResourceSchedulingPlanningObject::find(HMResourceSchedulingOpenTimeSlotsTmpLocal.HMResourceSchedulingPlanningObject));
        
            new MenuFunction(menuitemdisplaystr(HMResourceSchedulingAppointmentDetail), MenuItemType::Display).run(args);
        
            super();
        }

]]></Source>
