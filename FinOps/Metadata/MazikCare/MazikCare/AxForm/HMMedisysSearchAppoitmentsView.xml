			<Method>
				<Name>applyFilter</Name>
				<Source><![CDATA[
    public void applyFilter(boolean _allStatus)
    {
        HMResourceSchedulingOpenTimeSlotsSearch  openSlotSearchObj =  new HMResourceSchedulingOpenTimeSlotsSearch();
        HMResourceSchedulingOpenTimeSlotsTmp                        hmResourceSchedulingOpenTimeSlotsTmpLocal;
    
        HMResourceSchedulingOpenTimeSlotsTmp_1.clear();
        HMResourceSchedulingOpenTimeSlotsTmp_2.clear();
    
        if (_allStatus)
        {
    
            if (this.validateWrite())
            {
    
              hmResourceSchedulingOpenTimeSlotsTmpLocal = openSlotSearchObj.searchOpenTimeSlotPatientChart(FromDate.dateValue(),ToDate.dateValue(),
              true,element.args().record().RecId,PhysicianId,clinicId,HMScheduleTypeRecId);
              HMResourceSchedulingOpenTimeSlotsTmp_1.setTmpData(hmResourceSchedulingOpenTimeSlotsTmpLocal);
              hmResourceSchedulingOpenTimeSlotsTmpLocal = openSlotSearchObj.searchOpenTimeSlotPatientChart(FromDate.dateValue(),ToDate.dateValue()
              ,false,element.args().record().RecId,PhysicianId,clinicId,HMScheduleTypeRecId);
              HMResourceSchedulingOpenTimeSlotsTmp_2.setTmpData(hmResourceSchedulingOpenTimeSlotsTmpLocal);
    
            }
    
    
        }
    
            HMResourceSchedulingOpenTimeSlotsTmp_1_DS.executeQuery();
            HMResourceSchedulingOpenTimeSlotsTmp_2_DS.executeQuery();
    
    }

]]></Source>
