			<Method>
				<Name>updateButtons</Name>
				<Source><![CDATA[
    public void updateButtons()
    {
        HMPatientEODStatement_ds.object(fieldNum(HMPatientEODStatement,IntervalEndDateTime)).allowEdit(HMPatientEODStatement.Status == HMPatientEODStatementStatus::Open);
        HMPatientEODStatement_ds.object(fieldNum(HMPatientEODStatement,IntervalStartDateTime)).allowEdit(HMPatientEODStatement.Status == HMPatientEODStatementStatus::Open);
        HMPatientEODStatement_ds.object(fieldNum(HMPatientEODStatement,HMOrganizationalUnit)).allowEdit(HMPatientEODStatement.Status == HMPatientEODStatementStatus::Open);
        if(HMPatientEODStatement.Status == HMPatientEODStatementStatus::Open)
        {
            HMPatientEODStatementClass_CalcStmt.enabled(true);
            HMPatientEODStatementClass_PostStmt.enabled(false);
            HMPatientEODStatementClass_ClearStmt.enabled(false);
        }
        else if(HMPatientEODStatement.Status == HMPatientEODStatementStatus::Calculated)
        {
            HMPatientEODStatementClass_CalcStmt.enabled(false);
            HMPatientEODStatementClass_PostStmt.enabled(true);
            HMPatientEODStatementClass_ClearStmt.enabled(true);
    
        }
        else if(HMPatientEODStatement.Status == HMPatientEODStatementStatus::Posted)
        {
            HMPatientEODStatementClass_CalcStmt.enabled(false);
            HMPatientEODStatementClass_PostStmt.enabled(false);
            HMPatientEODStatementClass_ClearStmt.enabled(false);
        }
    }

]]></Source>
