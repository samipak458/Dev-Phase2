						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            HMLabProceduresObservationCode          hmLabProceduresObservationCode;
            HMLabObservation                        hmLabProceduresObservation;
            Object                                  formRunObject;
            FormRun                                 formRun;
            isformclose=true;
            hmLabProceduresObservation = HMLabObservation_ds.getFirst(true);
            while(hmLabProceduresObservation)
            {
                ttsBegin;
                hmLabProceduresObservationCode.initValue();
                hmLabProceduresObservationCode.HMLabObservation= hmLabProceduresObservation.RecId;
                hmLabProceduresObservationCode.InventTable=itemId;
                hmLabProceduresObservationCode.insert();
                ttsCommit;
                hmLabProceduresObservation = HMLabObservation_ds.getNext();
            }
            element.closeSelectRecord(HMLabObservation);
            formRun = element.args().caller().owner();
        
            if(formHasMethod(formRun,identifierstr(refreshObservations)))
                    {
                        formRunObject = formRun;
                        formRunObject.refreshObservations();
                    }
            super();
        }

]]></Source>
