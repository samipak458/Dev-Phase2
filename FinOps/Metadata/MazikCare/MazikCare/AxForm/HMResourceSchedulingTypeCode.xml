    public void updateParentApptType()
    {
        if(HMResourceSchedulingTypeCode.AppointmentSubType == HMAppointmentSubType::Open)
        {
            HMResourceSchedulingTypeCode_Ds.object(fieldNum(HMResourceSchedulingTypeCode, ParentAppointmentType)).allowEdit(true);
        }
        else
