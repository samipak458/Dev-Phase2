						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
            if(ret)
            {
                if (HMResourceSchedulingTypeCode.RecId)
                {
                    [red,green,blue] = WinAPI::RGBint2Con(HMResourceSchedulingTypeCode.ColourCode);
                    ColorCode.backgroundColor(WinAPI::RGB2int(100, 120, 0));
                }
                else
                {
                    ColorCode.backgroundColor(WinAPI::RGB2int(255, 255, 255));
                }
                
                element.updateParentApptType();
            }
            return ret;
        }

]]></Source>
