			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        ListEnumerator enumr = strSplit(element.args().parm(),',').getEnumerator();
    
        enumr.moveNext();
        day = enumr.current();
        enumr.moveNext();
        lastSelectedId = enumr.current();
    
        super();
        element.selectMode(HMResourceSchedulingDayPlan_Description);
    }

]]></Source>
