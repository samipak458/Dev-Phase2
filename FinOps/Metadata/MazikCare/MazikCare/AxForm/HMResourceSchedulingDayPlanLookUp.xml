    public void closeSelectRecord(Common _selectedRecord)
    {
        switch(day)
        {
            case '1':
                element.args().caller().StoreDay1(_selectedRecord.RecId);
                break;
            case '2':
                element.args().caller().StoreDay2(_selectedRecord.RecId);
                break;
            case '3':
                element.args().caller().StoreDay3(_selectedRecord.RecId);
                break;
            case '4':
                element.args().caller().StoreDay4(_selectedRecord.RecId);
                break;
            case '5':
                element.args().caller().StoreDay5(_selectedRecord.RecId);
                break;
            case '6':
                element.args().caller().StoreDay6(_selectedRecord.RecId);
                break;
            case '7':
                element.args().caller().StoreDay7(_selectedRecord.RecId);
                break;
        }
    
        super(_selectedRecord);
    }

]]></Source>
