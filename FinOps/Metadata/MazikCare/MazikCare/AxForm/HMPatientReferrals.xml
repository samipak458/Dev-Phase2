				<Source><![CDATA[
    public boolean isDiagnosisAdded()
    {
        HMPatientReferralDiagnosis  hmPatientReferralDiagnosisLocal;
        boolean ret = true;
    
        select count(RecId) from hmPatientReferralDiagnosisLocal
                where hmPatientReferralDiagnosisLocal.HMPatientReferrals == HMPatientReferrals.RecId;
    
        if(hmPatientReferralDiagnosisLocal.RecId == 0)
        {
            ret = false;
        }
    
        return ret;
    }

]]></Source>
