        public boolean modified()
        {
            boolean ret;
            RecId locationRecId;
        
            ret = super();
            if(ret)
            {
                ServiceLocation.text('');
                MerchantId.text('');
                serviceLocationRecId = 0;
                merchantRecId = 0;
                if(this.text() == '')
                {
                    element.storeOrganizationalUnit(0);
                }
                else
                {
                    locationRecId = HMResourceSchedulingHelper::getLocationRecId(this.text());
