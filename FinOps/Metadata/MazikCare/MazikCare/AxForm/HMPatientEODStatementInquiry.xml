					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            str             range;
            HMPatientEODStatement_ds.queryBuildDataSource().clearRanges();
            HMPatientEODStatement_ds.queryBuildDataSource().clearDynalinks();
            HMPatientEODStatement_ds.queryBuildDataSource().addRange(fieldNum(HMPatientEODStatement,Status)).value(SysQuery::value(HMPatientEODStatementStatus::Posted));
            if(organizationalLocationRecId != 0)
            {
                HMPatientEODStatement_ds.queryBuildDataSource().addRange(fieldNum(HMPatientEODStatement,HMOrganizationalUnit)).value(SysQuery::value(organizationalLocationRecId));
            }
            if(serviceLocationRecId != 0)
            {
                HMPatientEODStatement_ds.queryBuildDataSource().addRange(fieldNum(HMPatientEODStatement,HMOrganizationalUnit_ServiceLocation)).value(SysQuery::value(serviceLocationRecId));
            }
            if(merchantRecId != 0)
        
            {
        
                HMPatientEODStatement_ds.queryBuildDataSource().addRange(fieldNum(HMPatientEODStatement,HMMerchantDetails)).value(SysQuery::value(merchantRecId));
            }
            if(UserId.text())
            {
                HMPatientEODStatement_ds.queryBuildDataSource().addRange(fieldNum(HMPatientEODStatement,UserId)).value(SysQuery::value(UserId.text()));
            }
            //range = strFmt('((HMPatientEODStatement.IntervalStartDateTime >= %1) && (HMPatientEODStatement.IntervalEndDateTime <= %1)',DateTimeUtil::toStr(StartDateTime.dateTimeValue()));
            //range += strFmt(' && (HMPatientEODStatement.IntervalStartDateTime >= %1) && (HMPatientEODStatement.IntervalEndDateTime <= %1))',DateTimeUtil::toStr(EndDateTime.dateTimeValue()));
        
            if(StartDateTime.dateTimeValue() != utcDateTimeNull())
            {
                range = strFmt('((HMPatientEODStatement.IntervalStartDateTime >= %1)',DateTimeUtil::toStr(StartDateTime.dateTimeValue()));
            }
        
            if(EndDateTime.dateTimeValue() != utcDateTimeNull())
            {
                range += strFmt(' && (HMPatientEODStatement.IntervalEndDateTime <= %1))',DateTimeUtil::toStr(EndDateTime.dateTimeValue()));
            }
        
            if(range)
            {
                HMPatientEODStatement_ds.queryBuildDataSource().addRange(fieldNum(HMPatientEODStatement,RecId)).value(range);
            }
        
            HMPatientEODStatement_ds.executeQuery();
            if(!HMPatientEODStatement)
            {
                info("@MAZ12337");
            }
        }

]]></Source>
