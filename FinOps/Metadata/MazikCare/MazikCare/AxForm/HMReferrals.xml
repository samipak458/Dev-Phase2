			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        HMResourceSchedulingAppointment hmResourceSchedulingAppointment;
    
        hmResourceSchedulingAppointment = element.args().record();
    
        appointmentRecId = hmResourceSchedulingAppointment.RecId;
        caseRecId = hmResourceSchedulingAppointment.HMCase;
    
        super();
    
        this.refreshGrid();
    }

]]></Source>
			</Method>
			<Method>
				<Name>refreshGrid</Name>
				<Source><![CDATA[
    public void refreshGrid()
    {
        boolean ret;
        HMServiceAppHelper  appHelper = new HMServiceAppHelper();
        container conValues, recContainer;
        int recCount = 0, i =1;
        int enumVal;
        int64 recIdValue;
        Map recMap;
    
        try
        {
            delete_from HMReferralsTmp;
    
            ret = appHelper.callPostWebAPI('api/PatientOrder/getPatientReferral', "{'AxAppoitnmentRefRecId':'" + int642str(appointmentRecId) +"','currentPage' :'1','forFulfillment':'false'}");
    
            if(ret)
            {
                conValues = appHelper.parmRecordsCon();
    
                if(conValues != conNull())
                {
                    recCount = conPeek(conValues, 3);
    
                    if(recCount > 0)
                    {
                        for(i = 1;i <=recCount; i++)
                        {
                            recContainer = conPeek(conValues, i+3);
    
                            recMap = Map::create(recContainer);
    
                            HMReferralsTmp.clear();
    
                            HMReferralsTmp.AppointmentRecommendation = recMap.lookup('ApptrecommendationName');
                            HMReferralsTmp.Diagnosis = recMap.lookup('DiagnosisName');
                            HMReferralsTmp.ReferralOrderId = recMap.lookup('Id');
                            recIdValue = recMap.lookup('ResourceRecId');
                            HMReferralsTmp.HMResourceMaster = recIdValue;
                            recIdValue = recMap.lookup('PatientRecId');
                            HMReferralsTmp.HMPatient = recIdValue;
                            recIdValue = recMap.lookup('SpecialtyRecId');
                            HMReferralsTmp.HMSpecialtyCode = recIdValue;
                            enumVal = recMap.lookup('Category');
                            HMReferralsTmp.ReferralCategory = enumVal;
                            HMReferralsTmp.ReferralReason = recMap.lookup('ReferralComment');
                            HMReferralsTmp.ReferralTo = recMap.lookup('Referralto');
                            enumVal = recMap.lookup('ReferralType');
                            HMReferralsTmp.ReferralType = enumVal;
                            HMReferralsTmp.HMCase = caseRecId;
                            recIdValue = recMap.lookup('AppointmentRecId');
                            HMReferralsTmp.HMResourceSchedulingAppointment = recIdValue;
    
                            HMReferralsTmp.insert();
                        }
                    }
                }
                else
                {
                    ret = checkFailed("@MAZ13418");
                }
            }
            else
            {
                if(appHelper.parmErrorMessage())
                {
                    checkFailed(appHelper.parmErrorMessage());
                }
    
                ret = checkFailed("@MAZ13418");
            }
    
            HMReferralsTmp_ds.executeQuery();
        }
        catch
        {
            ret = false;
        }
    }

]]></Source>
