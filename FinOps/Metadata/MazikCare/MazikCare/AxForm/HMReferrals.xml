					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
        
            if(HMReferralsTmp.ReferralCategory == HMReferralCategory::External)
            {
                HMResourceSchedulingAppointmentReferral.enabled(false);
            }
            else
            {
                if(HMReferralsTmp.HMResourceSchedulingAppointment)
                {
                    HMResourceSchedulingAppointmentReferral.enabled(false);
                }
                else
                {
                    HMResourceSchedulingAppointmentReferral.enabled(true);
                }
            }
        
            return ret;
        }

]]></Source>
