					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            List salesIdList;
            ListEnumerator      listEnumerator;
            QueryBuildRange     qbr;
            container           conResult;
            HMPatient           hmPatient;
            super();
            formHandlerClass = new HMPatientCollectiveChargesFormHandler();
            formHandlerClass.parmPayerAppointmentRecId(element.args().record().RecId);
            salesIdList = formHandlerClass.getRelativesSalesIdListAndAmount();
            SalesLine_ds.queryBuildDataSource().clearDynalinks();
            if(salesIdList.elements() == 0)
            {
                element.close();
            }
            listEnumerator = salesIdList.getEnumerator();
        
            while(listEnumerator.moveNext())
            {
                SalesLine_ds.queryBuildDataSource().addRange(fieldNum(SalesLine,SalesId)).value(SysQuery::value(listEnumerator.current()));
            }
            conResult = formHandlerClass.getAmounts();
            if(conResult == conNull())
            {
                element.close();
            }
            TaxAmount.realValue(conPeek(conResult,1));
            TotalAmount.realValue(conPeek(conResult,2));
            PaymentResponsibility.text(hmPatient::find(str2int64(element.args().parm())).patientName());
        }

]]></Source>
