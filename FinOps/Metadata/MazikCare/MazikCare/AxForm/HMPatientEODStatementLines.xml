		<DataSources xmlns="">
			<DataSource>
				<Name>CustTrans</Name>
				<Methods>
					<Method>
						<Name>showSettledInvoices</Name>
						<Source><![CDATA[
        [SysClientCacheDataMethodAttribute(true)]
        public display str showSettledInvoices(CustTrans _custTrans)
        {
            CustSettlement  settlement;
            CustTrans       custTransInvoice;
            str             invoiceId;
        
            while select Recid from settlement
                where settlement.OffsetRecid == _custTrans.RecId
                join Invoice from custTransInvoice
                where settlement.TransCompany == custTransInvoice.dataAreaId
                && settlement.TransRecId == custTransInvoice.RecId
                && settlement.AccountNum == custTransInvoice.AccountNum
            {
                invoiceId += custTransInvoice.Invoice + ' ; ';
            }
        
            return invoiceId;
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
