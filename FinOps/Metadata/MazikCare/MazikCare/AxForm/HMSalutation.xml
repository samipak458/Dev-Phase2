        for(i=0;i<ListViewGender.getCount();i++)
        {
            item = ListViewGender.getItem(i);
            if(conFind(HMSalutation.Gender,item.text()))
            {
                item.stateChecked(true);
            }
            else
            {
                item.stateChecked(false);
            }
    
            ListViewGender.setItem(item);
        }
    }

]]></Source>
