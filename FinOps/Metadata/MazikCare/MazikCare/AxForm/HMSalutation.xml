			<Method>
				<Name>updateGender</Name>
				<Source><![CDATA[
    public void updateGender()
    {
        container con;
        FormListItem    item;
        int  i,j;
    
        j=1;
    
        for(i=0;i<ListViewGender.getCount();i++)
        {
            item = ListViewGender.getItem(i);
            if(item.stateChecked())
            {
                con = conIns(con,j,item.text());
                j++;
            }
        }
    
        HMSalutation.Gender = con;
        DisplayGender.update();
    }

]]></Source>
