    public void updateDesign()
    {
        if(HMSalutation)
        {
            GroupMaritalStatus.enabled(true);
            GroupGender.enabled(true);
        }
        else
        {
            GroupMaritalStatus.enabled(false);
            GroupGender.enabled(false);
        }
    }

]]></Source>
