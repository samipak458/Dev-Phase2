				<Source><![CDATA[
    public void initializeGenderListView()
    {
        SysDictEnum     dictEnum;
        int             i;
        int             itemCount;
        FormListItem item;
    
        dictEnum        = new SysDictEnum(enumNum(HcmPersonGender));
        itemCount       = dictEnum.values();
    
        for (i = 0; i < itemCount; i++)
        {
            item = new FormListItem();
    
            item.text(dictEnum.index2Symbol(i));
            item.data(dictEnum.index2Value(i));
            ListViewGender.addItem(item);
        }
    
    
    }

]]></Source>
