		<DataControls xmlns="">
			<Control>
				<Name>Close</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            HMResourceSchedulingAppointment     HMResourceSchedulingAppointmentMarked;
            Object                              formRunObject;
            FormRun                             formRun;
        
            super();
        
            if (HMResourceSchedulingAppointment_ds.recordsMarked().lastIndex() > 0)
            {
                HMResourceSchedulingAppointmentMarked = HMResourceSchedulingAppointment_ds.getFirst(1);
                while (HMResourceSchedulingAppointmentMarked.RecId)
                {
                    element.markDelayed(HMResourceSchedulingAppointmentMarked.RecId);
                    info(strFmt("@MAZ9472",HMResourceSchedulingAppointmentMarked.AppointmentNumber));
                    HMResourceSchedulingAppointmentMarked = HMResourceSchedulingAppointment_ds.getNext();
                }
            }
            else if (HMResourceSchedulingAppointment.RecId != 0)
            {
                element.markDelayed(HMResourceSchedulingAppointment.RecId);
                info(strFmt("@MAZ9472",HMResourceSchedulingAppointmentMarked.AppointmentNumber));
            }
        
            element.close();
            formRun = element.args().caller();
        
            if (formHasMethod(formRun, identifierstr(refreshGrid)))
            {
                    formRunObject = formRun;
                    formRunObject.refreshGrid();
            }
        }

]]></Source>
