	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class HMResourceSchedulingAppointmentDelayed extends FormRun
{
    RecId       planningObjectRecId, StaffOURecId;
    HMCaseType  caseType;
    container   param;
    int         treeType;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        str headerText;
    
        if (element.args().parm() != "")
        {
            param               = str2con(element.args().parm());
            caseType            = conPeek(param,1);
            StaffOURecId        = conPeek(param,2);
            planningObjectRecId = conPeek(param,3);
            treeType            = conPeek(param,4);
    
            if (planningObjectRecId)
            {
                if (treeType == 1)
                {
                    headerText = HMResourceSchedulingPlanningObject::find(planningObjectRecId).showOrganizationUnit() + ', ';
                }
                else if (treeType == 2)
                {
                    headerText = HMClinicStaff::find(HMResourceSchedulingPlanningObject::find(planningObjectRecId).HMClinicStaff).physicianName() + ', ';
                }
                else
                {
                    headerText = HMResourceSchedulingPlanningObject::find(planningObjectRecId).Description + ', ';
                }
    
            }
    
            if (StaffOURecId)
            {
                if (treeType == 1)
                {
                    headerText += HMClinicStaff::find(StaffOURecId).physicianName();
                }
                else
                {
                    headerText += HMOrganizationalUnit::find(StaffOURecId).Description;
                }
    
            }
    
            headerText = strFmt("@MAZ8363",headerText);
        }
        else
        {
            headerText = "@MAZ8361";
        }
    
        super();
    
        MainInstruction.text(headerText);
    }

]]></Source>
			</Method>
			<Method>
				<Name>markDelayed</Name>
				<Source><![CDATA[
    public void markDelayed(RecId  _appointmentRecId)
    {
        // mark appointment as delayed
        HMResourceSchedulingAppointment     HMResourceSchedulingAppointmentUpdate;
    
        select forUpdate HMResourceSchedulingAppointmentUpdate
            where HMResourceSchedulingAppointmentUpdate.RecId == _appointmentRecId;
    
        ttsBegin;
        HMResourceSchedulingAppointmentUpdate.IsDelayed = NoYes::Yes;
        HMResourceSchedulingAppointmentUpdate.update();
        ttsCommit;
    }

]]></Source>
			</Method>
		</Methods>
