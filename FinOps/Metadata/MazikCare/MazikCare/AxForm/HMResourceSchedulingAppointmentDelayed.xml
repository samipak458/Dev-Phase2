						<Source><![CDATA[
        public void init()
        {
            super();
        
            if (element.args().parm() != "")
            {
                if (caseType == HMCaseType::None)
                {
                    this.query().dataSourceTable(tableNum(HMResourceSchedulingPlanningObject)).addRange(fieldNum(HMResourceSchedulingPlanningObject,CaseType)).value(SysQuery::value(HMCaseType::OutPatient));
                    this.query().dataSourceTable(tableNum(HMResourceSchedulingPlanningObject)).addRange(fieldNum(HMResourceSchedulingPlanningObject,CaseType)).value(SysQuery::value(HMCaseType::Surgery));
                }
                if (StaffOURecId)
                {
                    if (treeType == 1)
                    {
                        this.query().dataSourceTable(tableNum(HMResourceSchedulingPlanningObject)).addRange(fieldNum(HMResourceSchedulingPlanningObject,HMClinicStaff)).value(SysQuery::value(StaffOURecId));
                    }
                    else
                    {
                        this.query().dataSourceTable(tableNum(HMResourceSchedulingPlanningObject)).addRange(fieldNum(HMResourceSchedulingPlanningObject,HMOrganizationalUnit)).value(SysQuery::value(StaffOURecId));
                    }
        
                }
                if (planningObjectRecId)
                {
                    this.query().dataSourceTable(tableNum(HMResourceSchedulingPlanningObject)).addRange(fieldNum(HMResourceSchedulingPlanningObject,RecId)).value(SysQuery::value(planningObjectRecId));
                }
            }
        }

]]></Source>
