				<Source><![CDATA[
    public void close()
    {
        FormRun             formRun;
        Object              formObj;
    
        super();
    
        formRun = element.args().caller().owner();
        formObj = formRun;
    
        if (formHasMethod(formObj, identifierstr(refreshChiefComplaint)))
        {
            formObj.refreshChiefComplaint();
        }
    }

]]></Source>
