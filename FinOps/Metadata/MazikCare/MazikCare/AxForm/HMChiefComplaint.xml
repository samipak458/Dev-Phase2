			<Method>
				<Name>saveRecords</Name>
				<Source><![CDATA[
    public void saveRecords()
    {
        HMClinicalObservation            HMClinicalObservationLocal;
        RecId                            organizationUnitId;
        HMOrganizationChiefComplaint     HMOrganizationChiefComplaintLocal;
    
        HMClinicalObservationLocal  = HMClinicalObservation_ds.getFirst(true);
        organizationUnitId          =  HMOrganizationalUnit::find(element.args().record().RecId).RecId;
    
        while(HMClinicalObservationLocal)
        {
            HMOrganizationChiefComplaintLocal.HMOrganizationalUnit = organizationUnitId;
            HMOrganizationChiefComplaintLocal.HMClinicalObservation = HMClinicalObservationLocal.RecId;
            HMOrganizationChiefComplaintLocal.insert();
            HMClinicalObservationLocal = HMClinicalObservation_ds.getNext();
        }
    }

]]></Source>
