					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
             HMResourceSchedulingAppointment   HMResourceSchedulingAppointmentLocal;
        
        
        
            HMResourceSchedulingOpenTimeSlotsSearch   HMResourceSchedulingOpenTimeSlotsSearch;
            HMResourceSchedulingOpenTimeSlotsTmp  hmResourceSchedulingOpenTimeSlotsTmpLocal;
        
            HMResourceSchedulingAppointmentLocal = HMResourceSchedulingAppointment::find( element.args().record().RecId);
            HMResourceSchedulingOpenTimeSlotsSearch = new HMResourceSchedulingOpenTimeSlotsSearch();
            ScheduleType.text(HMResourceSchedulingTypeCode::find( HMResourceSchedulingAppointmentLocal.HMResourceSchedulingTypeCode).Description);
        
            if (element.ValidateWrite())
            {
                hmResourceSchedulingOpenTimeSlotsTmpLocal = hmResourceSchedulingOpenTimeSlotsSearch.searchOpenTimeSlot(HMResourceSchedulingAppointmentLocal.HMResourceSchedulingPlanningObject,
                                                            FromDate.dateValue(),
                                                            ToDate.dateValue(),
                                                            false,
                                                            HMResourceSchedulingAppointmentLocal.HMResourceSchedulingTypeCode,
                                                            false,
                                                            [false,false,false,false,false,false,false]);
        
        
            delete_from hmResourceSchedulingOpenTimeSlotsTmpLocal
             where
                            (hmResourceSchedulingOpenTimeSlotsTmpLocal.Time < HMResourceSchedulingAppointmentLocal.AppointmentTime
                            &&  hmResourceSchedulingOpenTimeSlotsTmpLocal.Date == HMResourceSchedulingAppointmentLocal.AppointmentDate);
        
            delete_from HMResourceSchedulingOpenTimeSlotsTmp;
            HMResourceSchedulingOpenTimeSlotsTmp.setTmpData(hmResourceSchedulingOpenTimeSlotsTmpLocal);
            HMResourceSchedulingOpenTimeSlotsTmp_ds.executeQuery();
        
            }
            super();
        }

]]></Source>
