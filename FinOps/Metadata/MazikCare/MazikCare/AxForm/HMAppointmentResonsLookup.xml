						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            super();
            if(element.args().parm() == 'OT Order')
            {
                this.query().dataSourceTable(tableNum(ReasonTable)).addRange(fieldNum(ReasonTable,Ordering)).value(SysQuery::value(NoYes::Yes));
            }
            else if(element.args().parm() == 'CancelForBooked')
            {
                  this.query().dataSourceTable(tableNum(ReasonTable)).addRange(fieldNum(ReasonTable,ForceBookingCancel)).value(SysQuery::value(NoYes::Yes));
            }
            else
            {
                this.query().dataSourceTable(tableNum(ReasonTable)).addRange(fieldNum(ReasonTable,Appointments)).value(SysQuery::value(NoYes::Yes));
            }
        }

]]></Source>
