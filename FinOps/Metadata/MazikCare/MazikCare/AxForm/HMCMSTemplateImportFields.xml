			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        super();

        CommandButtonOk.enabled(false);
        FileUpload.style(FileUploadStyle::Standard);
        FileUpload.fileTypesAccepted('.xlsx');
    }

]]></Source>
