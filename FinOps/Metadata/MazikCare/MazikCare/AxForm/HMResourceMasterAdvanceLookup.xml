			<Method>
				<Name>loadImage</Name>
				<Source><![CDATA[
    boolean loadImage()
    {
         Image               imagelocal=new Image();
         container           con;
         container           blankImageContainer;
         Image               logoImage;
         DocuRef             documentRef;
         URL                 url;
    
        select * from documentref
        where documentref.RefRecId == HMResourceMaster.RecId
        && documentref.RefTableId == HMResourceMaster.TableId
        && documentref.HMAttachmentType == HMAttachmentDocType::Patient;
    
        if (documentRef.docuValue().File)
        {
            //todo: ax7 docu
            /*
            // file in database
      
            //todo: ax7 DocuAction     
            // DocuFileWatchDog::create(); //update old files
           // url = DocuActionFile::saveTempFile(documentRef);
            // htmlView.navigate(url);
            imagelocal.loadImage(url);
            imagelocal.resize(150, 150, InterpolationMode::InterpolationModeHighQuality);
            DoctorImage.image(imagelocal);
            DocuOpenFile::add(documentRef, url);
            DocuFileWatchDog::createDelayed(); // ensure that watchdog is started*/
        }
        else
        {
            //todo: ax7 DocuAction
            //url = documentRef.completeFilename();
            con = WinAPI::findFirstFile(url);
            if (!conpeek(con, 2) && conpeek(con, 1) == -1)
            {
                blankImageContainer = SysResource::getResourceNodeData(SysResource::getResourceNode(resourceStr('HcmWorkerPreviewPaneNoImage')));
                logoImage = new Image();
                logoImage.setData(blankImageContainer);
                logoImage.resize(150, 150, InterpolationMode::InterpolationModeHighQuality);
                DoctorImage.image(logoImage);
                return true;
            }
    
            //  htmlView.navigate(url);
            imagelocal.loadImage(url);
            imagelocal.resize(150,150, InterpolationMode::InterpolationModeHighQuality);
            DoctorImage.image(imagelocal);
        }
    
        return true;
    }

]]></Source>
