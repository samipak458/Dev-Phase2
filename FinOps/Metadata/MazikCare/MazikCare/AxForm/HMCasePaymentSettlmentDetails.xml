			<Method>
				<Name>run</Name>
				<Source><![CDATA[
    public void run()
    {
        container conBalances;
        HMCasePaymentSettlmentTmp tmpBuffer;
        HMCaseTrans     hmCaseTrans;
    
        tmpBuffer = (HMCasePaymentSettlmentTmp::fillData(element.args().record()));
        conBalances = HMCasePaymentSettlmentTmp::getBalances(tmpBuffer);
    
        BeginningBalance.realValue(conPeek(conBalances,1));
        SettledAmount.realValue(conPeek(conBalances,2));
        CurrentBalance.realValue(conPeek(conBalances,3));
    
        //accomodated to cater migarted beginning balance
        select sum(TotalAMount) from hmCaseTrans
        where hmCaseTrans.HMCase == element.args().record().RecId;
        BeginningBalance.realValue(hmCaseTrans.TotalAmount);
    
        HMCasePaymentSettlmentTmp.setTmpData(tmpBuffer);
        super();
    }

]]></Source>
			</Method>
