		<DataSources xmlns="" />
		<DataControls xmlns="">
			<Control>
				<Name>Symptoms</Name>
				<Type>String</Type>
				<Methods>
					<Method>
						<Name>modified</Name>
						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret;
            int i ;
            str strReasons;
            ret = super();
        
            if (ret)
            {
                if (this.valueStr() != "")
                {
                    cSymptoms = msCtrl.get();
                    for (i = 1; i <= conLen(cSymptoms);i++)
                    {
                        strReasons += strFmt(' %1(%2);', HMClinicalObservation::find(conPeek(cSymptoms,i)).Code,
                        HMClinicalObservation::find(conPeek(cSymptoms,i)).Description);
                    }
                }
                Symptoms.text(strReasons);
            }
        
            return ret;
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>Search</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            container                                   filterContainer;
            Object                                      formRunObject;
            FormRun                                     formRun;
        
            super();
            filterContainer=[FirstName.ValueStr(),LastName.ValueStr()];
        
            formRun = element.args().caller();
        
            if(firstname.valuestr() == '' && lastname.valuestr() == ''
               && physicianName.valueStr() == '' && (symptoms.valueStr() == '' && ReasonInpatient.valueStr() ==''))
            {
                box::warning("@SYS321784");
                return;
        
            }
        
            if (formHasMethod(formRun,identifierstr(refreshGrid)))
            {
                formRunObject = formRun;
                if (formType == HMCaseType::InPatient)
                    formRunObject.refreshGrid(FirstName.ValueStr(),LastName.ValueStr(),PhysicianName.ValueStr(), conNull(), ReasonInpatient.valueStr());
                else
                    formRunObject.refreshGrid(FirstName.ValueStr(),LastName.ValueStr(),PhysicianName.ValueStr(), cSymptoms);
            }
            element.close();
        
        }

]]></Source>
					</Method>
				</Methods>
