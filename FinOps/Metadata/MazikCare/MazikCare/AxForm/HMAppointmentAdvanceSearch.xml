}
]]></Source>
			</Method>
			<Method>
				<Name>clearAll</Name>
				<Source><![CDATA[
    public void clearAll()
    {
        chartnumber.text('');
        firstname.text('');
        lastname.text('');
        symptoms.text('');
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if (this.args())
        {
            formType =  str2enum(formType, element.args().parm());
            if (element.args().parm() == enum2str(HMCaseType::InPatient))
            {
                reasonInpatient.visible(true);
                Symptoms.visible(false);
    
            }
            else if (element.args().parm() == enum2str(HMCaseType::OutPatient))
            {
                Symptoms.visible(true);
                reasonInpatient.visible(false);
            }
        }
        msCtrl = SysLookupMultiSelectCtrl::construct(element, Symptoms, querystr(HMClinicalObservations));
    }

]]></Source>
			</Method>
			<Method>
				<Name>task</Name>
				<Source><![CDATA[
    public int task(int _taskId)
    {
       #task
        int ret;
    
        ret = super(_taskId);
    
        if (ret && _taskId == #TaskSave)
        {
            // Call appropriate saving here...
            msCtrl.get(); // get the Returned Ids
        }
    
        return ret;
    }

]]></Source>
			</Method>
