			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        HMPatient hmPatient;
        if(InclinicFileLocation.text() == "")
        {
            warning("@MAZ12297");
            return;
        }
        try
        {
            ttsBegin;
            select forupdate InClinicFileLocation from hmPatient
            where hmPatient.RecId == hmPatientRecId;
            if(hmPatient)
            {
                hmPatient.InClinicFileLocation = InclinicFileLocation.text();
                hmPatient.update();
    
                super();
                info("@MAZ12298");
                ttsCommit;
            }
            else
            {
                warning("@MAZ12299");
                ttsAbort;
            }
        }
        catch
        {
            warning("@MAZ12299");
        }
    
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        HMResourceSchedulingAppointment appt;
    
        appt = element.args().record();
        hmPatientRecId = appt.HMPatient;
    
        super();
    
    }

]]></Source>
