			<DataSource>
				<Name>HMCaseInsurance</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            this.query().dataSourceTable(tableNum(HMCaseInsurance)).addRange(fieldNum(HMCaseInsurance, HMCase)).value(queryValue(caseInsurance.HMCase));
        
            super();
        
            HMCaseInsurance_ds.positionToRecord(caseInsurance);
        }

]]></Source>
