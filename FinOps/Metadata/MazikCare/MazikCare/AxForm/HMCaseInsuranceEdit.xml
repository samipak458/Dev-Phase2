				<Source><![CDATA[
[Form]
public class HMCaseInsuranceEdit extends FormRun
{
    RefRecId          selectedCaseInsuranceRecId;
    HMCaseInsurance   caseInsurance;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        //Initialize splitter
    
        if (element.args().menuItemName(menuitemDisplayStr(HMCaseInsuranceEdit)))
        {
            if (element.args().record())
            {
                selectedCaseInsuranceRecId = element.args().record().RecId;
                caseInsurance = HMCaseInsurance::find(selectedCaseInsuranceRecId);
            }
    
        }
        else
        {
            warning("Form cannot be open with invalid record set.");
            element.close();
        }
    
    }

]]></Source>
			</Method>
		</Methods>
