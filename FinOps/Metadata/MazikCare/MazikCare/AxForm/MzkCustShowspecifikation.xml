			<Method>
				<Name>fillTmpdata</Name>
				<Source><![CDATA[
    public TmpCustVendTrans fillTmpdata(RecId _paymentRecId)
    {
        CustTrans           custVendTrans, custVendTransOffset;
        TmpCustVendTrans tmpCustVendTransLocal;

        while select * from  custVendTrans
                where   custVendTrans.Recid == _paymentRecId
        {
            tmpCustVendTransLocal.AccountNum             = custVendTrans.AccountNum;
            tmpCustVendTransLocal.Company                = custVendTrans.LastSettleCompany;
            tmpCustVendTransLocal.AmountCur              = custVendTrans.AmountCur;
            tmpCustVendTransLocal.CurrencyCode           = custVendTrans.CurrencyCode;
            tmpCustVendTransLocal.Invoice                = custVendTrans.Invoice;
            tmpCustVendTransLocal.BankRemittanceFileId   = custVendTrans.BankRemittanceFileId;
            tmpCustVendTransLocal.PaymReference          = custVendTrans.PaymReference;
            tmpCustVendTransLocal.PaymId                 = custVendTrans.PaymId;
            tmpCustVendTransLocal.PaymMode               = custVendTrans.PaymMode;
            tmpCustVendTransLocal.PostingProfile         = custVendTrans.PostingProfile;
            tmpCustVendTransLocal.TransDate              = custVendTrans.TransDate;
            tmpCustVendTransLocal.Txt                    = custVendTrans.Txt;
            tmpCustVendTransLocal.Voucher                = custVendTrans.Voucher;
            tmpCustVendTransLocal.insert();
        }

        while   select * from custVendTransOffset
                where   custVendTransOffset.RecId == _paymentRecId
                join    custVendTrans
                where   custVendTrans.OffsetRecid == custVendTransOffset.RecId
        {
            tmpCustVendTransLocal.AccountNum             = custVendTrans.AccountNum;
            tmpCustVendTransLocal.Company                = custVendTrans.LastSettleCompany;
            tmpCustVendTransLocal.AmountCur              = custVendTrans.AmountCur;
            tmpCustVendTransLocal.CurrencyCode           = custVendTrans.CurrencyCode;
            tmpCustVendTransLocal.Invoice                = custVendTrans.Invoice;
            tmpCustVendTransLocal.BankRemittanceFileId   = custVendTrans.BankRemittanceFileId;
            tmpCustVendTransLocal.PaymReference          = custVendTrans.PaymReference;
            tmpCustVendTransLocal.PaymId                 = custVendTrans.PaymId;
            tmpCustVendTransLocal.PaymMode               = custVendTrans.PaymMode;
            tmpCustVendTransLocal.PostingProfile         = custVendTrans.PostingProfile;
            tmpCustVendTransLocal.TransDate              = custVendTrans.TransDate;
            tmpCustVendTransLocal.Txt                    = custVendTrans.Txt;
            tmpCustVendTransLocal.Voucher                = custVendTrans.Voucher;
            tmpCustVendTransLocal.insert();
        }
    
        return tmpCustVendTransLocal;
    }

]]></Source>
