				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
        
            Object                              formRunObject;
            FormRun                             formRun;
            HMRecurrentApptDetail               HMRecurrentApptDetails;
        
            super();
        
            HMRecurrentApptDetails = HMRecurrentApptDetail_ds.getFirst(1);
            while (HMRecurrentApptDetails)
            {
                element.deleteAppointmentSeries(HMRecurrentApptDetails);
                HMRecurrentApptDetails = HMRecurrentApptDetail_ds.getNext();
            }
        
            element.refreshGrid();
        
            formRun = element.args().caller();
        
                if (formHasMethod(formRun, identifierstr(refreshGrid)))
                {
                     formRunObject = formRun;
                     formRunObject.refreshGrid();
                }
        
        }

]]></Source>
