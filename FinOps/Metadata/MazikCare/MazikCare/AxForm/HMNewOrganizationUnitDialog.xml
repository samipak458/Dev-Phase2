        public boolean modified()
        {
            boolean                      ret;
            HMOrganizationaCategory      hmOrganizationaCategory;
        
            ret = super();
        
            if (ret)
            {
                if (this.valueStr() != "")
                {
                    select RecId, Description from hmOrganizationaCategory
                    where hmOrganizationaCategory.Description == this.valueStr();
        
                    if (hmOrganizationaCategory.RecId)
                    {
                        element.setCategoryID(hmOrganizationaCategory.RecId);
                    }
                    else
                    {
                        info(strFmt("@SYS316757",'Category'));
                        element.setCategoryID(CategoryID);
        
                    }
                }
                else
                {
                    element.setCategoryID(0);
                }
        
            }
        
            return ret;
        }

