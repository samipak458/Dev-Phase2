        public void init()
        {
            QueryBuildDataSource qbds;
        
            super();
        
            qbds = this.queryBuildDataSource();
            qbds.addSortField(fieldNum(HMResourceSchedulingAppointment, AppointmentDate),SortOrder::Ascending);
            qbds.addSortField(fieldNum(HMResourceSchedulingAppointment, AppointmentTime),SortOrder::Ascending);
            qbds.addRange(fieldNum(HMResourceSchedulingAppointment, AppointmentStatus)).value(SysQuery::value(HMResourceSchedulingAppointmentStatus::Confirmed));
            qbds.addRange(fieldNum(HMResourceSchedulingAppointment, RecId)).value(
            strFmt('(((%1.%2 == %3) && (%1.%4 > %5)) || (%1.%2 > %3))',
            qbds.name(),
            fieldStr(HMResourceSchedulingAppointment, AppointmentDate),
            date2StrXpp(DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone())),
            fieldStr(HMResourceSchedulingAppointment, AppointmentTime),
            DateTimeUtil::getTimeNow(DateTimeUtil::getUserPreferredTimeZone())));
           // info(this.queryBuildDataSource().toString());
        
        }

]]></Source>
