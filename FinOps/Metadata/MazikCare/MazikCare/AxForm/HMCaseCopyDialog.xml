				<Source><![CDATA[
/* TODO: (Code Upgrade) Removed unused footnote group. */
[Form]
public class HMCaseCopyDialog extends FormRun
{
    RecId billTypeRecId;
     HMCase hmcase;
}
]]></Source>
			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        Args args;
        if(CaseType.selection() == 0)
        {
            warning("Select case type");
            return;
        }
        if(billTypeRecId == 0)
        {
            warning("Select bill type");
            return;
        }
        super();
        if(hmcase)
        {
            args = new Args();
            args.record(hmcase);
            args.caller(this);
            new MenuFunction(menuitemActionStr(HMCaseCopyRecordMenuItem),MenuItemType::Action).run(args);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>getBillingTypeRecId</Name>
				<Source><![CDATA[
    public RecId getBillingTypeRecId()
    {
        return billTypeRecId;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getCaseType</Name>
				<Source><![CDATA[
    public HMCaseType getCaseType()
    {
        return CaseType.selection();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
    
        HMCaseInsurance hmCaseINsurance;
        HMInsuranceCarrier  hmInsuranceCarrier;
        super();
        hmcase = element.args().record();
    
        select RecId,InsuranceCarrier from hmCaseINsurance
        where hmCaseINsurance.Priority == 1
        && hmCaseINsurance.IsActive == NoYes::Yes
        && hmCaseINsurance.hmcase == hmcase.RecId;
    
        if(!hmCaseINsurance)
        {
            warning("Primary insurance not active");
            element.close();
        }
    
        select AllowCopyCase from hmInsuranceCarrier
        where hmInsuranceCarrier.Code == hmCaseINsurance.InsuranceCarrier;
    
        if(hmInsuranceCarrier.AllowCopyCase == NoYes::No)
        {
            warning("Insurance carrier doesn't allow copy case");
            element.close();
        }
        CaseType.selection(hmcase.HMCaseType);
        billTypeRecId = hmcase.BillType;
        BillingType.text(HMClaimTableSetup::findByRecEnum(billTypeRecId,HMClaimEnumList::BillType).Description);
    }

]]></Source>
			</Method>
			<Method>
				<Name>storeClaimTableSetupRecId</Name>
				<Source><![CDATA[
    private void storeClaimTableSetupRecId(RecId _claimTableSetupRecId)
    {
        billTypeRecId = _claimTableSetupRecId;
    }

]]></Source>
			</Method>
