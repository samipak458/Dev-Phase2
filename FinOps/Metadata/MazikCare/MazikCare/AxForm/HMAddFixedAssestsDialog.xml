	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class HMAddFixedAssestsDialog extends FormRun
{
    RecId       buildingUnitRecId;
    Object      formRunObject;
    FormRun     formRun;
    NoYesId     isOkClicked;
}
]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        AssetTable  AssetTableLocal;
        AssetTable  AssetTableUpdate;
    
        super();
    
        if (isOkClicked == NoYes::Yes)
        {

            if (AssetTable_ds.recordsMarked().lastIndex() > 1)  // more than 1 marked records
            {
                AssetTableLocal = AssetTable_ds.getFirst(1);
    
                while (AssetTableLocal)
                {
                    ttsBegin;
                    select forUpdate RecId, HMBuildingUnit from AssetTableUpdate
                        where AssetTableUpdate.RecId == AssetTableLocal.RecId;
    
                    AssetTableUpdate.HMBuildingUnit = buildingUnitRecId;
                    AssetTableUpdate.update();
                    ttsCommit;
    
                    AssetTableLocal = AssetTable_ds.getNext();
                }
            }
            else  // if only one record is selected but not marked
            {
                ttsBegin;
                select forUpdate RecId, HMBuildingUnit from AssetTableUpdate
                    where AssetTableUpdate.RecId == AssetTable.RecId;
    
                AssetTableUpdate.HMBuildingUnit = buildingUnitRecId;
                AssetTableUpdate.update();
                ttsCommit;
            }
    
            formRun = element.args().caller();
    
            if (formHasMethod(formRun, identifierstr(refreshFixedAssetsGrid)))
            {
                formRunObject = formRun;
                formRunObject.refreshFixedAssetsGrid();
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        buildingUnitRecId = str2int64(element.args().parm());
        super();
    }

]]></Source>
			</Method>
