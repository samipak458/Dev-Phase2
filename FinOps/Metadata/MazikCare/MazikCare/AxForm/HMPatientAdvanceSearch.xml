        void clicked()
        {
               container                                   filterContainer;
               Object                                      formRunObject;
               FormRun                                     formRun;
        
        
            super();
            filterContainer=[ChartNumber.ValueStr(),FirstName.ValueStr(),LastName.ValueStr(),DateOfBirth.dateValue(),GovtID.ValueStr(),Phone.ValueStr(),PolicyNumber.ValueStr(),cmbGender.selection(),DateFrom.dateValue(),DateTo.dateValue()];
        
            formRun = element.args().caller();
        
        
            if(chartnumber.valuestr()=='' && firstname.valuestr()=='' && lastname.valuestr()==''
               && dateofbirth.datevalue()==datenull() &&
               govtid.valuestr()=='' && phone.valuestr()=='' && policynumber.valuestr()=='' && DateFrom.dateValue() == dateNull()  && DateTo.dateValue() == dateNull() && cmbGender.selection() == Gender::Unknown)
            {
                box::warning("@SYS321784");
                return;
        
            }
        
            if (formHasMethod(formRun,identifierstr(refreshGrid)))
            {
                 formRunObject = formRun;
                 formRunObject.refreshGrid(ChartNumber.ValueStr(),FirstName.ValueStr(),LastName.ValueStr(),DateOfBirth.dateValue(),GovtID.ValueStr(),Phone.ValueStr(),PolicyNumber.ValueStr(),cmbGender.selection(), DateFrom.dateValue(), DateTo.dateValue());
            }
        
            element.closeOk();
        
        }

]]></Source>
