        public boolean modified()
        {
            boolean ret;
        
        
            if(newarrivaltime.value() && NewArrivalTime.value()==-1)//!HMResourceSchedulingHelper::validateTimeNew(newarrivaltime.valueStr()))
            {
        
                ret = false;
        
                checkFailed("@MAZ11906");
        
            }
            else
            {
                ret = super();
            }
            return ret;
        }

]]></Source>
