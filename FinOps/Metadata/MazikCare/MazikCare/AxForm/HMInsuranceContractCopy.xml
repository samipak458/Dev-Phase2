        void clicked()
        {
             Object                              formRunObject;
            FormRun                             formRun;
             Args                                args;
            HmContractCopyObject HmContractCopyObject = new HmContractCopyObject();
            str abcd;
        
          if( ContractNumber.valueStr() == '')
            {
                throw error(" Number Missing");
            }
        
          if(contractName.valueStr() == '')
            {
                throw error(" Name Missing");
            }
        
          if(StartDate.dateValue()== dateNull())
            {
                throw error(" effective date Missing");
            }
           if(EndDate.dateValue() == dateNull())
            {
                throw error(" End date Missing");
            }
            //updating HMResourceSchedulingAppointment
        
        
        
            HmContractCopyObject.parmgetName(contractName.valueStr());
            HmContractCopyObject.parmgetNumber(ContractNumber.valueStr());
            HmContractCopyObject.parmgetStartDate(StartDate.dateValue());
            HmContractCopyObject.parmgetenddate(EndDate.dateValue());
            args = new args();
            args.caller(element);
            /*
            HMInsuranceContract.HMContractNumber = ContractNumber.valueStr();
            HMInsuranceContract.HMContractName= ContractNumber.valueStr();
            HMInsuranceContract.HMContractEffectiveDate = (StartDate.dateValue());
            HMInsuranceContract.HMContractEndDate = (enddate.dateValue());
            */
            args.parmObject(HmContractCopyObject);
            args.record(HMInsuranceContract);
            new MenuFunction(menuItemDisplayStr(HMInsuranceContractCopy), MenuItemType::Display).run(args);
        
            formRun = element.args().caller();
            formRun.dataSource().reread();
            abcd = formRun.name();
            formRun.dataSource().research();
             super();
        
        }

