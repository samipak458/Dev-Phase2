						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret;
        
            ret = super();
            if(ret)
            {
                HMCustomerServiceContractLines.TotalAmount = HMCustomerServiceContractLines_ds.showAmount(HMCustomerServiceContractLines);
            }
            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>showAmount</Name>
						<Source><![CDATA[
        public  Amount showAmount(HMCustomerServiceContractLines _lines)
        {
            BOM BOMLocal;
            select sum(Price) from  BOMLocal
                    where BOMLocal.BOMId == _lines.BOMTable;
        
            return   BOMLocal.Price;
        
        
        }

]]></Source>
					</Method>
					<Method>
						<Name>protocolName</Name>
						<Source><![CDATA[
        [SysClientCacheDataMethodAttribute(true)]
        public display Name protocolName(HMCustomerServiceContractLines _lines)
        {
            return BOMTable::find(_lines.BOMTable).Name;
        }

]]></Source>
