			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args() || !element.args().caller())
        {
            // Form cannot be called directly.
            throw error("@SYS96590");
        }   
        
        super();
        
        FileUpload.notifyUploadAttemptStarted += eventhandler(this.UploadStarted);
        FileUpload.notifyUploadCompleted += eventhandler(this.UploadCompleted);

        SecurityRights rights = SecurityRights::construct();
        if (rights.menuItemAccessRight(SecurableType::MenuItemDisplay, element.args().menuItemName(), curExt()) == AccessRight::View)
        {
            CancelButton.caption("@SYS127448");
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>UploadStarted</Name>
				<Source><![CDATA[
    /// <summary>
    /// Called when picture upload starts to upload image.
    /// </summary>
    private void UploadStarted()
    {
        OKButton.enabled(false);
    }

]]></Source>
			</Method>
			<Method>
				<Name>UploadCompleted</Name>
				<Source><![CDATA[
    /// <summary>
    /// Called when picture upload completes to store binary image.
    /// </summary>
    private void UploadCompleted()
    {
        OKButton.enabled(true);

        FileUploadTemporaryStorageResult fileUploadResult = FileUpload.getFileUploadResult();
        if(fileUploadResult != null && fileUploadResult.getUploadStatus())
        {
            //todo: ax 7 to be resolved after removal of oveerlayering
            //HMPatient.Image = ImageReference::GetPackedBinaryData(fileUploadResult.getDownloadUrl());
        }
        else
        {
            warning("@SYS89176");
        }
    }

]]></Source>
