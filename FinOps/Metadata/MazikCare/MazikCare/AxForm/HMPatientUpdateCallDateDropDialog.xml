			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        HMPatient hmPatient;
        if(CallDate.dateValue() == dateNull())
        {
            warning(SysLabel::labelId2String2('@MAZ11043'));
            return;
        }
        if(CallDate.dateValue() < DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone()))
        {
            warning(SysLabel::labelId2String2('@MAZ11044'));
            return;
        }
        try
        {
            ttsBegin;
            HMPatient = HMPatient::find(element.args().record().RecId,true);
            HMPatient.RecallDate = CallDate.dateValue();
            HMPatient.CallNotes = Notes.valueStr();
            HMPatient.update();
            ttsCommit;
            element.args().caller().refreshPatient();
        }
        catch
        {
            ttsAbort;
        }
    
        super();
    
    }

]]></Source>
			</Method>
