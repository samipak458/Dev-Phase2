        public boolean modified()
        {
            boolean ret;
        
            ret = super();
            if(ret)
            {
                HMClientProtocolPriceOverrideDetailsTmp_ds.object(fieldNum(HMClientProtocolPriceOverrideDetailsTmp,DiscountDistribution)).allowEdit(HMClientProtocolPriceOverrideDetailsTmp.Mark);
