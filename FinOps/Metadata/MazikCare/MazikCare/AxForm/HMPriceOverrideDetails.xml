				<Name>saveData</Name>
				<Source><![CDATA[
    public void saveData(RecId _clientProtocolRecId)
    {
        HMClientProtocolPriceClass.saveData(_clientProtocolRecId,HMClientProtocolPriceOverrideDetailsTmp);
    }

]]></Source>
