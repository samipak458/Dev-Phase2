			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
        if(HMClientProtocolPriceClass.validateData(HMClientProtocolPriceOverrideDetailsTmp))
        {
            validated = true;
            if(!hmClientProtocol.isNewRecord())
            {
                HMClientProtocolPriceClass.saveData(hmClientProtocol.RecId,HMClientProtocolPriceOverrideDetailsTmp);
            }
            super();
        }
        else
        {
            HMClientProtocolPriceOverrideDetailsTmp_ds.executeQuery();
        }
    
    }

]]></Source>
