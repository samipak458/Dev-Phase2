			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        BOMVersion bomversion;
        super();
        
        hmClientProtocol = element.args().record();
        HMClientProtocolPriceClass = new HMClientProtocolPriceClass();
        HMClientProtocolPriceOverrideDetailsTmp.setTmpData(HMClientProtocolPriceClass.fillProtocolPriceDetails(hmClientProtocol));
        HMClientProtocolPriceOverrideDetailsTmp_ds.executeQuery();
        select firstonly bomVersion
            where bomVersion.ItemId == hmClientProtocol.ItemId
            && bomVersion.Active
            && bomVersion.FromDate <= DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone())
            && (!bomVersion.ToDate || bomVersion.ToDate >= DateTimeUtil::getSystemDate(DateTimeUtil::getUserPreferredTimeZone()));
        HMClientProtocolPriceOverrideDetailsTmp_ds.allowEdit(!bomVersion.Active);
    }

]]></Source>
			</Method>
