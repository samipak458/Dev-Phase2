					<Method>
						<Name>validateWrite</Name>
						<Source><![CDATA[
        public boolean validateWrite()
        {
            boolean ret;
        
            ret = super();
            if(ret)
            {
                if(HMClientProtocolPriceOverrideDetailsTmp.Mark == NoYes::Yes)
                {
                    if(HMClientProtocolPriceOverrideDetailsTmp.DiscountDistribution <= 0)
                    {
                        warning(strFmt("%1 distribution must be greater than zero",HMClientProtocolPriceOverrideDetailsTmp.ItemId));
                        ret = false;
                    }
                }
            }
            return ret;
        }

]]></Source>
