		<DataControls xmlns="">
			<Control>
				<Name>SetCompleted</Name>
				<Type>MenuFunctionButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            if(smmActivityTaskTimeType.selection() == smmActivityTaskTimeType::Completed && HMActivitySubStatus.selection() == HMActivitySubStatus::None)
            {
                warning("@MAZ12511");
                return;
            }
            if(HMActivitySubStatus.selection() == HMActivitySubStatus::AutoEmail)
            {
                warning("@MAZ12479");
                return;
            }
            if(smmActivityTaskTimeType.selection() == smmActivityTaskTimeType::Deferred
            || smmActivityTaskTimeType.selection() == smmActivityTaskTimeType::InProgress
            || smmActivityTaskTimeType.selection() == smmActivityTaskTimeType::NotStarted
            || smmActivityTaskTimeType.selection() == smmActivityTaskTimeType::Waiting)
            {
                warning("@MAZ12480");
                return;
            }
            super();
            element.close();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
