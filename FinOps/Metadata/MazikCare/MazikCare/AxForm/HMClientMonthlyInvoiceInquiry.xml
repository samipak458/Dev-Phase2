					<Method>
						<Name>protocolAlias</Name>
						<Source><![CDATA[
        [SysClientCacheDataMethodAttribute(true)]
        public display EcoResDescription protocolAlias(CustInvoiceTrans _custInvoiceTrans)
        {
            HMClientProtocol hmClientProtocol;
        
            select * from hmClientProtocol
            where hmClientProtocol.ClientAccount == _custInvoiceTrans.custInvoiceJour().InvoiceAccount
            && hmClientProtocol.ItemId == _custInvoiceTrans.ProtocolId;
        
            if(hmClientProtocol)
            {
                return  hmClientProtocol.itemAlias();
            }
            return HMInventTableClass::showDescription(_custInvoiceTrans.ProtocolId);
        }

