				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            str range;
            super();
            if (FromDate.dateValue() == dateNull())
            {
                warning(strFmt("@SYS26332","@SYS24050"));
                return;
            }
        
            if (ToDate.dateValue() == dateNull())
            {
                warning(strFmt("@SYS26332","@SYS36680"));
                return;
            }
        
            if (FromDate.dateValue() > ToDate.dateValue())
            {
                warning("@MAZ2029");
                return;
            }
            if(HMClientMonthlyAccount.text() == '')
            {
                warning(strFmt("@SYS26332","@MAZ12347"));
                return;
            }
            CustInvoiceTrans_ds.queryBuildDataSource().clearRanges();
            qbrDate.value(SysQuery::range(FromDate.dateValue(),ToDate.dateValue()));
            qbrClient.value(HMClientMonthlyAccount.text());
            CustInvoiceJour_ds.executeQuery();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
