    public void init()
    {
        str patientGuid;
        Common common;
        FormRun caller;
        HMResourceSchedulingAppointment hmResourceSchedulingAppoinment;
        common = element.args().record();
        caller = element.args().caller();
    
        hmResourceSchedulingAppoinment = common;
    
        if (caller.args().menuItemName() == menuitemDisplayStr(HMPatientChargeEntryAdvancePaymentPharm))
        {
            HMPharmacyClinicDataHelper::delDiagnosis();
            if (hmResourceSchedulingAppoinment)
            {
                //patientGuid = HMPharmacyClinicDataHelper::getGuid(int642str(hmResourceSchedulingAppoinment.HMPatient));
                if (hmResourceSchedulingAppoinment.RecId)
                {
                    HMPharmacyClinicDataHelper::setPatientDiagnosis(int642str(hmResourceSchedulingAppoinment.RecId));
                }
            }
        }
    
        super();
    }

]]></Source>
