				<Methods>
					<Method>
						<Name>displayOption</Name>
						<Source><![CDATA[
        public void displayOption(Common _record, FormRowDisplayOption _options)
        {
            HMAnesthesiaType ResourceSchedulingTypeCode = _record;
        
            if (_record.RecId)
            {
                _options.backColor(ResourceSchedulingTypeCode.ColourCode);
                _options.affectedElementsByControl(WindowColour.id());
            }
            else
            {
                _options.backColor(WinAPI::RGB2int(255,255,255));
                _options.affectedElementsByControl(WindowColour.id());
            }
        
            super(_record, _options);
        }

]]></Source>
					</Method>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        public int active()
        {
            int ret;
        
            ret = super();
            if(ret)
            {
                if (HMAnesthesiaType.RecId)
                {
                    [red,green,blue] = WinAPI::RGBint2Con(HMAnesthesiaType.ColourCode);
                    WindowColour.backgroundColor(WinAPI::RGB2int(red, green, blue));
                }
                else
                {
                    WindowColour.backgroundColor(WinAPI::RGB2int(255, 255, 255));
                }
        
        
            }
            return ret;
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
