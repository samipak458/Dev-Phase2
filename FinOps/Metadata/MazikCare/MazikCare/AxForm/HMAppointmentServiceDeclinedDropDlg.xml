			</Method>
			<Method>
				<Name>closeOk</Name>
				<Source><![CDATA[
    public void closeOk()
    {
    
        if(ServiceDeclinedReason.text() == '')
        {
            warning("@MAZ1989");
            return;
        }
    
        if(element.args().parm() == 'AppointmentTemp')
        {
            this.parmDeclineReason(ServiceDeclinedReason.text());
            super();
        }
        else
        {
            if(HMResourceSchedulingHelper::declineService(ServiceDeclinedReason.text(),element.args().record().RecId))
            {
                super();
            }
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>parmDeclineReason</Name>
				<Source><![CDATA[
    public HMServiceDeclinedReason parmDeclineReason(HMServiceDeclinedReason _declineReason = declineReason)
    {
        declineReason = _declineReason;
    
        return declineReason;
    }

]]></Source>
