		<Methods>
			<Method>
				<Name>delete</Name>
				<Source><![CDATA[
    public void delete()
    {
        HMPatientInsurance HMPatientInsurance;
        HMPatient HMPatient;
    
        select *from HMPatientInsurance where HMPatientInsurance.HMInsuranceContract == this.RecId;
        if(HMPatientInsurance)
        {
             select *from HMPatient where HMPatient.RecId == HMPatientInsurance.HMPatient;
            throw error("Contract is assigned to patient (MRN ::" + HMPatient.getMRN() +" )" );
        }
        else
        {
        super();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>CopyContract</Name>
				<Source><![CDATA[
    public static HMInsuranceContract CopyContract(HMInsuranceContract _contract ,HmContractCopyObject _HmContractCopyObject )
    {
        HMInsuranceContract copyContract;
    
        //HmContractCopyObject _HmContractCopyObject = new HmContractCopyObject();
    
        //_HmContractCopyObject = HmContractCopyObject;
        copyContract.HMAllServicesCoveredInContract         = _contract.HMAllServicesCoveredInContract;
        copyContract.HMContractEffectiveDate                = _HmContractCopyObject.parmgetStartDate();
        copyContract.HMContractEndDate                      = _HmContractCopyObject.parmgetenddate();
        copyContract.HMContractName                         = _HmContractCopyObject.parmgetName();
        copyContract.HMContractNumber                       = _HmContractCopyObject.parmgetNumber();
        copyContract.HMContractStatus                       = _contract.HMContractStatus;
        copyContract.HMDeductiblePerService                 = _contract.HMDeductiblePerService;
        copyContract.HMDeductibleRequired                   = _contract.HMDeductibleRequired;
        copyContract.HMInsuranceCarrier                     = _contract.HMInsuranceCarrier;
        copyContract.HMInsuranceContractCarrierCombination  = _contract.HMInsuranceContractCarrierCombination;
    
        return copyContract;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static HMInsuranceContract find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMInsuranceContract  hmInsuranceContract;
    
        hmInsuranceContract.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmInsuranceContract.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmInsuranceContract where hmInsuranceContract.RecId == _recid;
    
        return hmInsuranceContract;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findByCarrierCombo</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public static HMInsuranceContract findByCarrierCombo(
    RecId               _carrierCombination,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMInsuranceContract  hmInsuranceContract;
    
        hmInsuranceContract.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmInsuranceContract.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmInsuranceContract where hmInsuranceContract.HMInsuranceContractCarrierCombination == _carrierCombination;
    
        return hmInsuranceContract;
    }

]]></Source>
