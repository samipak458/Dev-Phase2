		<Methods>
			<Method>
				<Name>showCodeRangeUnit</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display LongDescription showCodeRangeUnit()
    {
        LongDescription                     codeRangeUnit;
    
        codeRangeUnit=this.Code + ' ' +  HMMmtUnitCode::find(this.HMMmtUnitCode).Description;
    
        return codeRangeUnit;
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    public static boolean checkExist(RecId _recId)
    {
        boolean ret = true;
    
        if (!HMLabObservation::exist(_recId))
        {
            ret = checkFailed(strfmt(HMLabObservation::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(RecId _recId)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     hmLabObservation
                 where
                     hmLabObservation.RecId == _recId).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static HMLabObservation find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMLabObservation hmLabObservation;
    
        hmLabObservation.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmLabObservation.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly hmLabObservation where hmLabObservation.RecId == _recid;
    
        return hmLabObservation;
    }

]]></Source>
			</Method>
