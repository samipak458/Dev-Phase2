			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Indicates whether the specified record in the <c>HMClaim</c> table exists.
    /// </summary>
    /// <param name="_recId">
    /// The unique ID of the record to check for existence.
    /// </param>
    /// <returns>
    /// true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean exist(recId _recId)
    {
        boolean exist;
    
        exist = (select firstonly
                    RecId
                from
                    hmClaim
                where
                    hmClaim.RecId == _recId).RecId != 0;
    
        return exist;
    }

]]></Source>
