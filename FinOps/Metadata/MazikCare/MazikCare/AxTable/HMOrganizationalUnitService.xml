			<Method>
				<Name>findbyOUandService</Name>
				<Source><![CDATA[
    public static HMOrganizationalUnitService findbyOUandService(RecId _organizationalUnitRecId,
    Itemid                _itemId,
    boolean                _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMOrganizationalUnitService  hmOrganizationalUnitService;
    
        hmOrganizationalUnitService.selectForUpdate(_forUpdate);
    
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmOrganizationalUnitService.concurrencyModel(_concurrencyModel);
        }
    
        select firstOnly * from hmOrganizationalUnitService
            where hmOrganizationalUnitService.HMOrganizationalUnit == _organizationalUnitRecId
            && hmOrganizationalUnitService.InventTable == _itemId;
    
    
        return hmOrganizationalUnitService;
    }

]]></Source>
