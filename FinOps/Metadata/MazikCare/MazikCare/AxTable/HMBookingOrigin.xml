				<Source><![CDATA[
    /// <summary>
    ///     Finds the specified record in the <c>HMBookingOrigin</c> table.
    /// </summary>
    /// <param name="_recid">
    ///     The record ID of the <c>HMBookingOrigin</c> table to find.
    /// </param>
    /// <param name="_forUpdate">
    ///     A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///     The concurrency model option that will be used when it reads the record; optional.
    /// </param>
    /// <returns>
    ///     A record of the <c>HMBookingOrigin</c> table if found; otherwise, an empty buffer.
    /// </returns>
    public static HMBookingOrigin find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMBookingOrigin  hmBookingOrigin;
    
        hmBookingOrigin.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmBookingOrigin.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmBookingOrigin where hmBookingOrigin.RecId == _recid;
    
        return hmBookingOrigin;
    }

]]></Source>
