		<Methods>
			<Method>
				<Name>getItemRevenueCode</Name>
				<Source><![CDATA[
    public static RefRecId getItemRevenueCode(ItemId _itemId)
    {
        HMPatientChargeCombination combination;
        HMProcedureCodeMaster      procedureCode;
        int                        counter = 0;
        RefRecId                   revenueCode, preRevenueCode;
        boolean                    isFirst = true;
    
        while select combination
            where combination.ItemId == _itemId
        {
            if (isFirst == true)
            {
                counter++;
                revenueCode = combination.RevenueCode;
                preRevenueCode = revenueCode;
                isFirst = false;
            }
            else if(preRevenueCode != revenueCode)
            {
                counter++;
                revenueCode = combination.RevenueCode;
                preRevenueCode = revenueCode;
                isFirst = false;
            }
    
        }
    
        if (counter == 1)
        {
            return revenueCode;
        }
        else
        {
            return 0;
        }
    }

]]></Source>
			</Method>
