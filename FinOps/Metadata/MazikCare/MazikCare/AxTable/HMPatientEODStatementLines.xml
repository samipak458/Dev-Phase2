		<Methods>
			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    public static boolean checkExist(recId _recId)
    {
        boolean ret = true;
    
        if (!HMPatientEODStatementLines::exist(_recId))
        {
            ret = checkFailed(strFmt(HMPatientEODStatementLines::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(recId _recId)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     HMPatientEODStatementLines
                 where
                     HMPatientEODStatementLines.RecId == _recId).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static HMPatientEODStatementLines find(
        recId              _recId,
        boolean            _forupdate = false,
        ConcurrencyModel   _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMPatientEODStatementLines HMPatientEODStatementLines;
    
        HMPatientEODStatementLines.selectForUpdate(_forupdate);
        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMPatientEODStatementLines.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly
            HMPatientEODStatementLines
        where
            HMPatientEODStatementLines.RecId == _recId;
    
        return HMPatientEODStatementLines;
    }

]]></Source>
