		<Methods>
			<Method>
				<Name>createData</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public static HMClaimBatchJobsLogs createData(HMClaimJobType _jobType
        , HMClaimNumber _claimID
        , str _oldValue
        , str _newValue
        , int _batchThreadNumber
        , HMClaimProcessingStatus _claimProcessingStatus
        , str _errorLog
        , RecId _hmClaimBatchJobLogsHeader)
    {
        HMClaimBatchJobsLogs logs;

        logs.clear();
        logs.JobType                    = _jobType;
        logs.ClaimID                    = _claimID;
        logs.OldValue                   = _oldValue;
        logs.NewValue                   = _newValue;
        logs.BatchThreadNumbers         = _batchThreadNumber;
        logs.ClaimProcessingStatus      = _claimProcessingStatus;
        logs.ErrorLog                   = _errorLog;
        logs.ErrorLog                  += HMClaimBatchJobsLogs::getInfoLogStr();
        Logs.HMClaimBatchJobLogsHeader  = _hmClaimBatchJobLogsHeader;
        
        return logs;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getInfoLogStr</Name>
				<Source><![CDATA[
    public static str getInfoLogStr(int _startLine = 1)
    {
        int currentLine;
        str retValue;

        for(currentLine = _startLine; currentLine <= infolog.line(); currentLine++)
        {
            if(infolog.line() == currentLine)
            {
                retValue += infolog.text(currentLine);
            }
            else
            {
                retValue += infolog.text(currentLine) + '\n';
            }
        }

        return retValue;
    }

]]></Source>
			</Method>
