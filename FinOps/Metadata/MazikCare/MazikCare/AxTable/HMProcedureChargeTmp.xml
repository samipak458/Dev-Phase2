    /// </param>
    /// <param name="_forUpdate">
    ///     A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///     The concurrency model option that will be used when it reads the record; optional.
    /// </param>
    /// <returns>
    ///     A record of the <c>HMProcedureChargeTmp</c> table if found; otherwise, an empty buffer.
    /// </returns>
    public static HMProcedureChargeTmp find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMProcedureChargeTmp  hmProcedureChargeTmp;
        hmProcedureChargeTmp.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmProcedureChargeTmp.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmProcedureChargeTmp where hmProcedureChargeTmp.RecId == _recid;
    
        return hmProcedureChargeTmp;
    }

]]></Source>
