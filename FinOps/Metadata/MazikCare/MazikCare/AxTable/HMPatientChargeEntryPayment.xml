		<Methods>
			<Method>
				<Name>getPatientAccount</Name>
				<Source><![CDATA[
    public CustAccount getPatientAccount()
    {
        HMPatient                       hmpatient;
    
        select CustTable from hmpatient
        Where hmpatient.RecId == this.hmpatient;
    
        return hmpatient.patientChartNum();
    }

]]></Source>
			</Method>
			<Method>
				<Name>getPatientAccountByAppointment</Name>
				<Source><![CDATA[
    public CustAccount getPatientAccountByAppointment()
    {
        HMResourceSchedulingAppointment HMResourceSchedulingAppointment;
        HMPatient                       hmpatient;
    
        select CustTable from hmpatient
        join HMResourceSchedulingAppointment where hmpatient.RecId == HMResourceSchedulingAppointment.HMPatient
        && HMResourceSchedulingAppointment.RecId == this.HMResourceSchedulingAppointment;
    
        return hmpatient.patientChartNum();
    }

]]></Source>
			</Method>
			<Method>
				<Name>totalAmount</Name>
				<Source><![CDATA[
    public static real totalAmount(SalesId _salesId)
    {
        HMPatientChargeEntryPayment payment;
    
        select sum(Amount) from payment
        where payment.SalesId == _salesId;
    
        return payment.Amount;
    
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Created by Zain
    /// </summary>
    public static HMPatientChargeEntryPayment find(RecId _recId)
    {
        HMPatientChargeEntryPayment hmPatientChargeEntryPayment;

        select * from hmPatientChargeEntryPayment
            where hmPatientChargeEntryPayment.RecId == _recId;

        return hmPatientChargeEntryPayment;
    }

]]></Source>
			</Method>
		</Methods>
