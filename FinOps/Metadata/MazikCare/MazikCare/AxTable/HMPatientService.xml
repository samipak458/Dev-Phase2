			<Method>
				<Name>displayInstruction</Name>
				<Source><![CDATA[
    public display HMCarePlanInstruction displayInstruction()
    {
        HMCarePlanProcedure     hmCarePlanProcedure;
    
        select * from hmCarePlanProcedure
            where hmCarePlanProcedure.HMPatientService == this.RecId;
        //todo:
            //hmCarePlanProcedure.HMPatientEncounter == HMPatientEncounter::find(this.HMPatientEncounter).ParentEncounter
            //&&
    
        return hmCarePlanProcedure.Instruction;
    }

]]></Source>
			</Method>
			<Method>
				<Name>displayPerformedTime</Name>
				<Source><![CDATA[
    public display String30 displayPerformedTime()
    {
        if(this.PerformedDate == dateNull())
        {
            return "";
        }
        else
        {
            return strDel(time2str(this.PerformedTime,TimeSeparator::Colon,TimeFormat::AMPM),6,3);
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>isCarePlanProcedure</Name>
				<Source><![CDATA[
    public boolean isCarePlanProcedure()
    {
        HMCarePlanProcedure     hmCarePlanProcedure;
    
        select RecId, HMPatientService, HMPatientEncounter from hmCarePlanProcedure
            where hmCarePlanProcedure.HMPatientService == this.RecId;
          //Todo:      && hmCarePlanProcedure.HMPatientEncounter == HMPatientEncounter::find(this.HMPatientEncounter).ParentEncounter;
    
        if (hmCarePlanProcedure.RecId)
        {
            return true;
        }
        else
        {
            return false;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>isProcedurePerformd</Name>
				<Source><![CDATA[
    public boolean isProcedurePerformd(RecId  _encounterRecId)
    {
        HMPatientServicePerformedLog    hmPatientServicePerformedLog;
    
        select * from hmPatientServicePerformedLog
            where hmPatientServicePerformedLog.HMPatientService == this.RecId
            && hmPatientServicePerformedLog.HMPatientEncounter == _encounterRecId
            && hmPatientServicePerformedLog.Date != dateNull();
    
        if (hmPatientServicePerformedLog.RecId)
        {
            return true;
        }
        else
        {
            return false;
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Validates that the specified record exists in the <c>HMPatientService</c> table; if not, an error message is displayed.
    /// </summary>
    /// <param name="_recId">
    /// The unique ID of the record to check for existence.
    /// </param>
    /// <returns>
    /// true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean checkExist(recId _recId)
    {
        boolean ret = true;
    
        if (!HMPatientService::exist(_recId))
        {
            ret = checkFailed(strfmt(HMPatientService::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    /// Indicates whether the specified record in the <c>HMPatientService</c> table exists.
    /// </summary>
    /// <param name="_recId">
    /// The unique ID of the record to check for existence.
    /// </param>
    /// <returns>
    /// true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean exist(recId _recId)
    {
        boolean existHMPatientService;
    
        existHMPatientService = (select firstonly
                    RecId
                from
                    hmPatientService
                where
                    hmPatientService.RecId == _recId).RecId != 0;
    
        return existHMPatientService;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the specified record in the <c>HMPatientService</c> table.
    /// </summary>
    /// <param name="_recId">
    /// The unique ID of the record to find.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional
    /// </param>
    /// <returns>
    /// A record in the <c>HMPatientService</c> table; otherwise, an empty record.
    /// </returns>
    public static HMPatientService find(
        recId   _recId,
        boolean _forUpdate = false)
    {
        HMPatientService hmPatientService;
        if (_recId)
        {
            hmPatientService.selectForUpdate(_forUpdate);
    
            select firstonly hmPatientService
                where hmPatientService.RecId == _recId;
        }
        return hmPatientService;
    }

]]></Source>
