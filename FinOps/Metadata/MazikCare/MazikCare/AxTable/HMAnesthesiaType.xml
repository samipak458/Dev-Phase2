    /// </param>
    /// <param name="_concurrencyModel">
    ///    The concurrency model option to use when reading the record; optional.
    /// </param>
    /// <returns>
    ///    A record in the <c>HMAnesthesiaType</c> table; otherwise, an empty record.
    /// </returns>
    public static HMAnesthesiaType find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMAnesthesiaType   HMAnesthesiaType;
    
        HMAnesthesiaType.selectForUpdate(_forUpdate );
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMAnesthesiaType.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from HMAnesthesiaType
            where HMAnesthesiaType.RecId == _recid;
    
        return HMAnesthesiaType;
    }

