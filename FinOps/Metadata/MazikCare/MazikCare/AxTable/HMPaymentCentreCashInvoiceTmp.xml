			<Method>
				<Name>setCustTransField</Name>
				<Source><![CDATA[
    public void setCustTransField()
    {
        AmountMST amount;
        CustTrans custTrans;
        CustTransOpen custTransOpen;
        CustTable custTable;
    
        while select AmountCur from custTransOpen
        join Invoice,AccountNum from custTrans
        join custTable
        where custTransOpen.RefRecId == custTrans.RecId
        && custTransOpen.AccountNum == custTrans.AccountNum
        && custTrans.hmcase == this.HMCase
        && custTable.AccountNum == custTrans.AccountNum
        && custTable.CustType == CustType::Patient
        && custtrans.TransType == LedgerTransType::Cust
        {
            this.AmoutDueByPatient += abs(custTransOpen.AmountCur);
            this.Invoice = CustTrans.Invoice;
            this.CustAccount = custTrans.AccountNum;
        }
    
    }

]]></Source>
