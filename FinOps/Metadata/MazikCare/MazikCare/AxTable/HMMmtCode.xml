				<Name>checkExist</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Determines whether the specified record is located in the <c>HMMmtCode</c> table.
    /// </summary>
    /// <param name="_recId">
    ///    The record ID of the <c>HMMmtCode</c> table record to check if it exists.
    /// </param>
    /// <returns>
    ///    true if the specified record exists; otherwise, false.
    /// </returns>
    /// <remarks>
    ///    If the specified record is not located in the <c>HMMmtCode</c> table, an error message is
    ///    displayed.
    /// </remarks>
    public static boolean checkExist(RecId _recId)
    {
        boolean ret = true;
    
        if (!HMMmtCode::exist(_recId))
        {
            ret = checkFailed(strfmt(HMMmtCode::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Determines whether the specified record in the <c>HMMmtCode</c> table exists.
    /// </summary>
    /// <param name="_recId">
    ///    The record ID of the <c>HMMmtCode</c> table to check if it exists.
    /// </param>
    /// <returns>
    ///    true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean exist(RecId _recId)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     hmMmtCode
                 where
                     hmMmtCode.RecId == _recId).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    ///     Finds the specified record in the <c>HMMmtCode</c> table.
    /// </summary>
    /// <param name="_recid">
    ///     The record ID of the <c>HMMmtCode</c> table to find.
    /// </param>
    /// <param name="_forUpdate">
    ///     A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///     The concurrency model option that will be used when it reads the record; optional.
    /// </param>
    /// <returns>
    ///     A record of the <c>HMMmtCode</c> table if found; otherwise, an empty buffer.
    /// </returns>
    public static HMMmtCode find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMMmtCode  hmMmtCode;
    
        hmMmtCode.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmMmtCode.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmMmtCode where hmMmtCode.RecId == _recid;
    
        return hmMmtCode;
    }

]]></Source>
