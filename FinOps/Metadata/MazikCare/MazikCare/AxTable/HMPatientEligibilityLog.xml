				<Name>getCreatedByName</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    display Name getCreatedByName()
    {
        UserInfo userInfo;
    
        select * from userInfo where userInfo.id == this.createdBy;
    
        return userInfo.name;
    }

]]></Source>
			</Method>
			<Method>
				<Name>isPatientInsuranceEligible</Name>
				<Source><![CDATA[
    public static boolean isPatientInsuranceEligible(RefRecId _recId)
    {
        boolean isEligible;
        HMPatientEligibilityLog log;
    
        select EligibilityStatus, RecId, CreatedDateTime
            from log
                order by RecId desc
                    where log.HMPatientInsurance == _recId;
    
        if (DateTimeUtil::date(DateTimeUtil::applyTimeZoneOffset(log.CreatedDateTime, DateTimeUtil::getUserPreferredTimeZone())) == DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone()))
        {
            if (!log.EligibilityStatus)
            {
                error("Patient is not eligible for this insurance carrier.");
            }
            return log.EligibilityStatus;
        }
        else
        {
            //error(strFmt("There is no eligibility check performed for the patient today, check patient eligibility.", DateTimeUtil::applyTimeZoneOffset(log.createdDateTime, DateTimeUtil::getUserPreferredTimeZone())));
            return true;
        }
    }

]]></Source>
			</Method>
