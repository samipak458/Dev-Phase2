		<Methods>
			<Method>
				<Name>findByRange</Name>
				<Source><![CDATA[
    public static HMAppointmentReserveSLot findByRange(TransDate _fromDate, TransDate _toDate, TimeOfDay _fromTime, TimeOfDay _toTime, RecId _resourceRecId, HMAppointmentSlotReserveBlock _task)
    {
        HMAppointmentReserveSLot hmAppointmentReserveSLot;
    
        select * from hmAppointmentReserveSLot
                where hmAppointmentReserveSLot.Date >= _fromDate
                && hmAppointmentReserveSLot.Date <= _toDate
                && hmAppointmentReserveSLot.StartTime >= _fromTime
                && hmAppointmentReserveSLot.EndTime <= _toTime
                && hmAppointmentReserveSLot.SlotReserveBlock == _task
                && hmAppointmentReserveSLot.HMResourceMaster == _resourceRecId;
    
        return hmAppointmentReserveSLot;
    }

]]></Source>
			</Method>
		</Methods>
	</SourceCode>
