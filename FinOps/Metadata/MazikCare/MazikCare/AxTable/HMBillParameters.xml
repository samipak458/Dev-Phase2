			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Determines whether the specified record in the <c>HMBillParameters</c> table exists.
    /// </summary>
    /// <param name="_recId">
    ///    The record ID of the <c>HMBillParameters</c> table to check if it exists.
    /// </param>
    /// <returns>
    ///    true if the specified record exists; otherwise, false.
    /// </returns>
    public static boolean exist(RecId _recId)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     HMBillParameters
                 where
                     HMBillParameters.RecId == _recId).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    static HMBillParameters find(boolean _forupdate=false)
    {
        HMBillParameters parameter;
    
        parameter.selectForUpdate(_forupdate);
    
        select firstonly parameter  index Key  where parameter.Key == 0;
    
        if (!parameter && !parameter.isTmp())
        {
            Company::createParameter(parameter);
        }
    
        return parameter;
    }

]]></Source>
