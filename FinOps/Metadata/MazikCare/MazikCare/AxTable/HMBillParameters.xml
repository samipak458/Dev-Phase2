			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean ret;
    
        ret = super();
    
        if (ret)
        {
            if (InventTable::find(this.FreeFollowUpService).itemGroupId() != this.ConsultationItemGroup)
            {
                ret = false;
                error(strFmt('Free follow up service should belong to the default consultation group %1', this.ConsultationItemGroup));
            }
        }
    
        return ret;
    }

]]></Source>
