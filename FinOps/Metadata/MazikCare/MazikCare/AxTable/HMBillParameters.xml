			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Determines whether the specified record is located in the <c>HMBillParameters</c> table.
    /// </summary>
    /// <param name="_recId">
    ///    The record ID of the <c>HMBillParameters</c> table record to check if it exists.
    /// </param>
    /// <returns>
    ///    true if the specified record exists; otherwise, false.
    /// </returns>
    /// <remarks>
    ///    If the specified record is not located in the <c>HMBillParameters</c> table, an error message is
    ///    displayed.
    /// </remarks>
    public static boolean checkExist(RecId _recId)
    {
        boolean ret = true;
    
        if (!HMBillParameters::exist(_recId))
        {
            ret = checkFailed(strfmt(HMBillParameters::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
