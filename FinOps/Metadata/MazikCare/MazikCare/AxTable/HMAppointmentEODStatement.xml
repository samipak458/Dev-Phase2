			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        boolean ret;
    
        ret = super();
        if(ret)
        {
            if(this.Status != HMAppointmentEODStatementStatus::Open)
            {
                warning("@MAZ11747");
                ret = false;
            }
        }
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    public static boolean checkExist(HMAppointmentEODStatementNumber _statementNumber)
    {
        boolean ret = true;
    
        if (!HMAppointmentEODStatement::exist(_statementNumber))
        {
            ret = checkFailed(strFmt(HMAppointmentEODStatement::txtNotExist(), _statementNumber));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(HMAppointmentEODStatementNumber _statementNumber)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     HMAppointmentEODStatement
                 where
                     HMAppointmentEODStatement.StatementNumber == _statementNumber).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static HMAppointmentEODStatement find(
        HMAppointmentEODStatementNumber _statementNumber,
        boolean            _forupdate = false,
        ConcurrencyModel   _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMAppointmentEODStatement HMAppointmentEODStatement;
    
        HMAppointmentEODStatement.selectForUpdate(_forupdate);
        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMAppointmentEODStatement.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly
            HMAppointmentEODStatement
        where
            HMAppointmentEODStatement.StatementNumber == _statementNumber;
    
        return HMAppointmentEODStatement;
    }

]]></Source>
