			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Finds the specified record in the <c>HMMovementType</c> table.
    /// </summary>
    /// <param name="_recid">
    ///    The record ID of the <c>HMMovementType</c> record to find.
    /// </param>
    /// <param name="_forUpdate">
    ///    A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///    The concurrency model option to use when reading the record; optional.
    /// </param>
    /// <returns>
    ///    A record in the <c>HMMovementType</c> table; otherwise, an empty record.
    /// </returns>
    public static HMMovementType find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMMovementType   hmMovementType;
    
        hmMovementType.selectForUpdate(_forUpdate );
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmMovementType.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmMovementType
            where hmMovementType.RecId == _recid;
    
        return hmMovementType;
    }

]]></Source>
