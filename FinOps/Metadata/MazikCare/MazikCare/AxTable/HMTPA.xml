				<Source><![CDATA[
    /// <summary>
    ///    Finds the specified record in the <c>HMTPA</c> table.
    /// </summary>
    /// <param name="_recId">
    ///    The record ID of the <c>HMTPA</c> record to find.
    /// </param>
    /// <param name="_forUpdate">
    ///    A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///    The concurrency model option to use when reading the record; optional.
    /// </param>
    /// <returns>
    ///    A record in the <c>HMTPA</c> table; otherwise, an empty record.
    /// </returns>
    public static HMTPA find(
    RecId     _RecId,
    boolean                     _forUpdate = false,
    ConcurrencyModel            _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMTPA   HMTPA;
    
        HMTPA.selectForUpdate(_forUpdate );
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMTPA.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly HMTPA
            where HMTPA.RecId == _RecId;
    
        return HMTPA;
    }

