		<Methods>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    static HMSmsMessageTable find(
        SysEmailId _id,
        LanguageId _language,
        boolean    _forUpdate = false)
    {
        HMSmsMessageTable sysEmailMessageTable;
        ;
    
        if (_id)
        {
            sysEmailMessageTable.selectForUpdate(_forUpdate);
    
            select firstonly sysEmailMessageTable
                index hint IdLanguageIdIdx
                where sysEmailMessageTable.HMSmsTable == _id &&
                      sysEmailMessageTable.LanguageId == _language;
        }
        return sysEmailMessageTable;
    }

]]></Source>
			</Method>
