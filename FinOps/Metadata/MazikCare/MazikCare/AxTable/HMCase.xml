				<Name>caseActiveInsuranceExists</Name>
				<Source><![CDATA[
    //NS: Bug 3277:Fti generation and posting is taking too much time by Ammar.Ali
    /// <summary>
    ///Checks whether active insurance exists on a case
    /// </summary>
    public boolean caseActiveInsuranceExists()
    {
        HMCaseInsurance caseInsurance;
        
        select firstonly RecId from caseInsurance order by caseInsurance.Priority ASC
            where caseInsurance.IsActive && caseInsurance.HMCase == this.RecId;

        return caseInsurance.RecId ? true : false;
    }

]]></Source>
