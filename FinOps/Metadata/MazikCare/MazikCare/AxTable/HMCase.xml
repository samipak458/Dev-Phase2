			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        HMResourceSchedulingAppointment appt;
        CustTrans                       custTrans;
        HMCaseTrans                     hmCaseTrans;
        HMCaseTransDetail               hmCaseTransDetail;
    
        boolean ret;
    
        ret = super();
        if(ret)
        {
            select firstOnly RecId from appt
            where appt.HMCase == this.RecId;
    
            if(appt)
            {
                ret = checkFailed("Case made from appointment cannot be deleted");
            }
    
            select firstOnly RecId from custTrans
            where custTrans.HMCase == this.RecId;
    
            if(custTrans)
            {
                ret = checkFailed("Case cannot be deleted, Financial transaction has been made.");
            }
    
            select firstOnly RecID from hmCaseTransDetail
            join hmCaseTrans
            where hmCaseTransDetail.HMCaseTrans == hmCaseTrans.RecId
            && hmCaseTrans.HMCase == this.RecId;
    
            if(hmCaseTransDetail)
            {
                ret = checkFailed("Case cannot be deleted, Financial/inventory transaction exist.");
            }
    
        }
        return ret;
    }

]]></Source>
