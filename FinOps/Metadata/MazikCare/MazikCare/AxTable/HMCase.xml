				<Name>modifiedField</Name>
				<Source><![CDATA[
    public void modifiedField(FieldId _fieldId)
    {
        HMCaseType caseType;
        super(_fieldId);
        switch (_fieldId)
        {
            case fieldNum(HMCase,HMPatientStatus):
                caseType = HMPatientStatusCaseType::findCaseByPatientStatus(this.HMPatientStatus) ;
                if(caseType == HMCaseType::None)
                {
                    warning("Selected patient status is not associated with case type");
                    this.HMPatientStatus = this.orig().HMPatientStatus;
                }
                else
                {
                    this.HMCaseType = caseType;
                }
                break;
        }
    }

]]></Source>
