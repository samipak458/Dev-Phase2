				<Source><![CDATA[
    /// <summary>
    ///    Finds the specified record in the <c>HMCase</c> table.
    /// </summary>
    /// <param name="_RecId">
    ///    The record ID of the <c>HMCase</c> record to find.
    /// </param>
    /// <param name="_forUpdate">
    ///    A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///    The concurrency model option to use when reading the record; optional.
    /// </param>
    /// <returns>
    ///    A record in the <c>HMCase</c> table; otherwise, an empty record.
    /// </returns>
    public static HMCase find(
    RecId     _RecId,
    boolean                     _forUpdate = false,
    ConcurrencyModel            _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMCase   HMCase;
    
        HMCase.selectForUpdate(_forUpdate );
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMCase.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly HMCase
            where HMCase.RecId == _RecId;
    
        return HMCase;
    }

]]></Source>
