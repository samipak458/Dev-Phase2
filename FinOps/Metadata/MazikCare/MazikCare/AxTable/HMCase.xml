			<Method>
				<Name>getInsuranceCarrierRecId</Name>
				<Source><![CDATA[
    public RecID getInsuranceCarrierRecId()
    {
        HMInsuranceCarrier	carrier;
        HMCaseInsurance		caseInsurance;
        HMPatientInsurance	patientInsurance;
    
        select RecId from carrier
            join caseInsurance
			join patientInsurance
            where caseInsurance.InsuranceCarrier	== carrier.Code
            && caseInsurance.HMPatientInsurance		== patientInsurance.RecId
			&& caseInsurance.HMCase					== this.RecId;
    
        return carrier.RecId;
    }

]]></Source>
