				<Name>update</Name>
				<Source><![CDATA[
    public void update()
    {
        str notes;
        super();
        if(this.HMFinancialClass != this.orig().HMFinancialClass)
        {
            notes = strFmt("Financial class updated from %1 to %2", HMFinancialClass::find(this.orig().HMFinancialClass).HMCode, HMFinancialClass::find(this.HMFinancialClass).HMCode);
            HMCaseNotes::createCaseIdentificationNotes(this.RecId,HMCaseNotesIdentification::FinClassUpdate,notes);
        }
    }

]]></Source>
