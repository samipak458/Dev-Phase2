				<Name>findByRecEnum</Name>
				<Source><![CDATA[
    public static HMClaimTableSetup findByRecEnum(RecId _recId, HMClaimEnumList _enumList)
    {
        HMClaimTableSetup hmClaimSetup;
    
        select firstOnly1 hmClaimSetup
            where  hmClaimSetup.RecId == _recId
                && hmClaimSetup.HMClaimEnumList == _enumList;
    
        return hmClaimSetup;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findByCode</Name>
				<Source><![CDATA[
    public static HMClaimTableSetup findByCode(str code)
    {
        HMClaimTableSetup hmClaimTableSetup;

        select firstonly hmClaimTableSetup
            where hmClaimTableSetup.Code == code;

        return hmClaimTableSetup;
    }

]]></Source>
