		<Methods>
			<Method>
				<Name>purchAmount</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display AmountCur purchAmount()
    {
        purchTotals purchTotals;
        PurchTable purchTableLocal;
        purchTableLocal = PurchTable::find(this.PurchTable);
        if(purchTableLocal.PurchStatus == PurchStatus::Backorder)
        {
            purchTotals = PurchTotals::newPurchTable(purchTableLocal);
            purchTotals.calc();
            return purchTotals.purchTotalAmount();
        }
        else
        {
            return purchTableLocal.amountInvoiced();
        }
    }

]]></Source>
			</Method>
