		<Methods>
			<Method>
				<Name>findNextInvoiceAction</Name>
				<Source><![CDATA[
    public static HMInvoiceNextAction findNextInvoiceAction(RecId _recId)
    {
        HMEOBReasonCode hmEOBReasonCode;
    
        select firstonly InvoiceNextAction from HMEOBReasonCode
        where HMEOBReasonCode.RecId == _recId;
    
        return hmEOBReasonCode.InvoiceNextAction;
    }

]]></Source>
			</Method>
			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    public static RecId findRecId(HMCode _code)
    {
        HMEOBReasonCode buffer;
    
        select RecId from buffer
        where buffer.Code == _code;
    
        return buffer.RecId;
    }

]]></Source>
			</Method>
