			<Method>
				<Name>showChiefComplaints</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display Str1260 showChiefComplaints()
    {
        int counter;
        str chiefComplaints;
        RecId recId;
    
    
        container chiefComplaintContainer = this.ChiefComplaint;
    
        for (counter = 1; counter <= conLen(chiefComplaintContainer);counter++)
        {
            if (counter == 1)
            {
                recId = conPeek(chiefComplaintContainer,counter);
                chiefComplaints  = HMClinicalObservation::find(recId).Description;
            }
            else
            {
                recId = conPeek(chiefComplaintContainer,counter);
                chiefComplaints += '; ' + HMClinicalObservation::find(recId).Description;
            }
    
        }
    
        return chiefComplaints;
    }

]]></Source>
