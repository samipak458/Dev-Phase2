				<Name>checkExist</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Determines whether the specified record is located in the <c>HMWaitingList</c> table.
    /// </summary>
    /// <param name="_recId">
    ///    The record ID of the <c>HMWaitingList</c> table record to check if it exists.
    /// </param>
    /// <returns>
    ///    true if the specified record exists; otherwise, false.
    /// </returns>
    /// <remarks>
    ///    If the specified record is not located in the <c>HMWaitingList</c> table, an error message is
    ///    displayed.
    /// </remarks>
    public static boolean checkExist(RecId _recId)
    {
        boolean ret = true;
    
        if (!HMWaitingList::exist(_recId))
        {
            ret = checkFailed(strfmt(HMWaitingList::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
