			<Method>
				<Name>patientName</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    display DirPartyName patientName()
    {
        if (this.HMPatient == 0)
        {
            return this.UnregPatientName + '*';
        }
        else
        {
            return HMPatient::find(this.HMPatient).patientName();
        }
    
    }

]]></Source>
