				<Name>departmentRecId</Name>
				<Source><![CDATA[
    public recid departmentRecId()
    {
        RecId departmentRecId;
    
        if(this.WaitType == HMWaitType::OP)
        {
            departmentRecId = HMResourceSchedulingPlanningObject::find(this.HMResourceSchedulingPlanningObject).HMOrganizationalUnit;
        }
    
        return departmentRecId;
    }

]]></Source>
