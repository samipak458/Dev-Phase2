			<Method>
				<Name>apptEmailExist</Name>
				<Source><![CDATA[
    public static boolean apptEmailExist(RecId _appointmentRecId, RecId _emailEventId)
    {
        HMResourceSchedulingApptEmailDetails HMResourceSchedulingApptEmailDetails;
        select * from HMResourceSchedulingApptEmailDetails
        where HMResourceSchedulingApptEmailDetails.HMEmailTemplatesEventMapping == _emailEventId
        && HMResourceSchedulingApptEmailDetails.HMResourceSchedulingAppointment == _appointmentRecId;
    
        if(HMResourceSchedulingApptEmailDetails.RecId)
        {
            return true;
        }
        return false;
    }

]]></Source>
