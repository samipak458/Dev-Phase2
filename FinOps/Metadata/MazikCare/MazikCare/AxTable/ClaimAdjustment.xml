		<Methods>
			<Method>
				<Name>getPatientId</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display CustAccount getPatientId()
    {
        CustTable   custTable;
        HMPatient   hmPatient;
    
        select AccountNum from custTable 
            join hmPatient
            where hmPatient.RecId == this.HmPatient
            && custTable.RecId == hmPatient.CustTable;
    
        return custTable.AccountNum;
    }

]]></Source>
