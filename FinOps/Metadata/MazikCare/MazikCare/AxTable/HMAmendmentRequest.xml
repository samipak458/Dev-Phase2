				<Name>hasRequestLines</Name>
				<Source><![CDATA[
    public boolean hasRequestLines()
    {
        HMAmendmentRequestLine      hmAmendmentRequestLine;
        boolean                     ret;
    
        select count (RecId) from hmAmendmentRequestLine
            where hmAmendmentRequestLine.HMAmendmentRequest == this.RecId;
    
        if (hmAmendmentRequestLine.RecId > 0)
        {
            ret = true;
        }
    
        return ret;
    }

]]></Source>
