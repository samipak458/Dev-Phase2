			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    ///    Finds the specified record in the <c>HMAmendmentRequest</c> table.
    /// </summary>
    /// <param name="_recid">
    ///    The record ID of the <c>HMAmendmentRequest</c> record to find.
    /// </param>
    /// <param name="_forUpdate">
    ///    A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    ///    The concurrency model option to use when reading the record; optional.
    /// </param>
    /// <returns>
    ///    A record in the <c>HMAmendmentRequest</c> table; otherwise, an empty record.
    /// </returns>
    public static HMAmendmentRequest find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMAmendmentRequest   hmAmendmentRequest;
    
        hmAmendmentRequest.selectForUpdate(_forUpdate );
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmAmendmentRequest.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmAmendmentRequest
            where hmAmendmentRequest.RecId == _recid;
    
        return hmAmendmentRequest;
    }

]]></Source>
