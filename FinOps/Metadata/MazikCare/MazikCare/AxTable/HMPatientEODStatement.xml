		<Methods>
			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        boolean ret;
    
        ret = super();
        if(ret)
        {
            if(this.Status != HMPatientEODStatementStatus::Open)
            {
                warning("@MAZ11747");
                ret = false;
            }
        }
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    public static boolean checkExist(HMAppointmentEODStatementNumber _statementNumber)
    {
        boolean ret = true;
    
        if (!HMPatientEODStatement::exist(_statementNumber))
        {
            ret = checkFailed(strFmt(HMPatientEODStatement::txtNotExist(), _statementNumber));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(HMAppointmentEODStatementNumber _statementNumber)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     HMPatientEODStatement
                 where
                     HMPatientEODStatement.StatementNumber == _statementNumber).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static HMPatientEODStatement find(
        HMPatientEODStatementNumber _statementNumber,
        boolean            _forupdate = false,
        ConcurrencyModel   _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMPatientEODStatement HMPatientEODStatement;
    
        HMPatientEODStatement.selectForUpdate(_forupdate);
        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMPatientEODStatement.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly
            HMPatientEODStatement
        where
            HMPatientEODStatement.StatementNumber == _statementNumber;
    
        return HMPatientEODStatement;
    }

]]></Source>
