				<Name>find</Name>
				<Source><![CDATA[
    public static HMAppointmentChannel find(
    RecId               _recid,
    boolean             _forUpdate = false,
    ConcurrencyModel    _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMAppointmentChannel  hmAppointmentChannel;
    
        hmAppointmentChannel.selectForUpdate(_forUpdate);
    
        if(_forUpdate  && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmAppointmentChannel.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly * from hmAppointmentChannel where hmAppointmentChannel.RecId == _recid;
    
        return hmAppointmentChannel;
    }

]]></Source>
