		<Methods>
			<Method>
				<Name>CPTCode</Name>
				<Source><![CDATA[
    display HMProcedureMasterCode CPTCode()
    {
        HMProcedureMasterCode  itemDescription = "";
    
        itemDescription = HMInventTable::findByItemId(this.ItemId).HMProcedureMasterCode;
        return itemDescription;
    }

]]></Source>
			</Method>
			<Method>
				<Name>itemDescription</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display Description itemDescription()
    {
        InventTable inventTable;
        EcoResProduct ecoResProduct;
    
        select ItemId, Product from inventTable
        where inventTable.ItemId == this.ItemId;
    
        return inventTable.itemDescriptionOrName();
    }

]]></Source>
			</Method>
			<Method>
				<Name>serviceDescription</Name>
				<Source><![CDATA[
    display HMProcedureMasterDescription serviceDescription()
    {
        HMProcedureMasterDescription  itemDescription = "";
    
        itemDescription = HMInventTableClass::showDescription(this.ItemId);
        return itemDescription;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setProtocolDescription</Name>
				<Source><![CDATA[
    public Description setProtocolDescription(CustAccount _clientAccount)
    {
        HMClientProtocol hmClientProtocol;
    
        select * from hmClientProtocol
        where hmClientProtocol.ClientAccount == _clientAccount
        && hmClientProtocol.ItemId == this.ProtocolId;
    
        if(hmClientProtocol)
        {
            return hmClientProtocol.itemAlias();
        }
        return HMInventTableClass::showDescription(this.ProtocolId);
    
    }

]]></Source>
			</Method>
			<Method>
				<Name>showDuration</Name>
				<Source><![CDATA[
    public display Description showDuration()
    {
        return strFmt('%1 %2',this.DurationPeriod,enum2str(this.PhysioDuration));
    }

]]></Source>
			</Method>
			<Method>
				<Name>showFrequency</Name>
				<Source><![CDATA[
    public display Description showFrequency()
    {
        return strFmt('%1 time per %2 %3',this.FrequencyTimes,this.FrequencyPeriod,enum2str(this.PhysioFrequency));
    }

]]></Source>
			</Method>
		</Methods>
