			<Method>
				<Name>validateDelete</Name>
				<Source><![CDATA[
    public boolean validateDelete()
    {
        boolean         ret;
        utcdatetime     minDateTime = DateTimeUtil::utcNow();
        HMClaimVersion  version;
    
        select firstonly validtimestate(minDateTime) * from version
            order by ValidTo desc
            where version.HMClaimTable == this.RecId;
    
        if (!version || version.Status == HMClaimStatus::Generated || version.Status == HMClaimStatus::InReview || version.Status == HMClaimStatus::SubmitReady)
        {
            ret = super();
        }
        else
        {
            ret = false;
            error("@MAZ13479");
        }
        return ret;
    }

]]></Source>
