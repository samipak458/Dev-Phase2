		<Methods>
			<Method>
				<Name>checkExist</Name>
				<Source><![CDATA[
    public static boolean checkExist(recId _recId)
    {
        boolean ret = true;
    
        if (!HMPatientEODStatementSummaryLines::exist(_recId))
        {
            ret = checkFailed(strFmt(HMPatientEODStatementSummaryLines::txtNotExist(), _recId));
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    public static boolean exist(recId _recId)
    {
        boolean found;
    
        found = (select firstonly
                     RecId
                 from
                     HMPatientEODStatementSummaryLines
                 where
                     HMPatientEODStatementSummaryLines.RecId == _recId).RecId != 0;
    
        return found;
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static HMPatientEODStatementSummaryLines find(
        recId              _recId,
        boolean            _forupdate = false,
        ConcurrencyModel   _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMPatientEODStatementSummaryLines HMPatientEODStatementSummaryLines;
    
        HMPatientEODStatementSummaryLines.selectForUpdate(_forupdate);
        if (_forupdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            HMPatientEODStatementSummaryLines.concurrencyModel(_concurrencyModel);
        }
    
        select firstonly
            HMPatientEODStatementSummaryLines
        where
            HMPatientEODStatementSummaryLines.RecId == _recId;
    
        return HMPatientEODStatementSummaryLines;
    }

]]></Source>
