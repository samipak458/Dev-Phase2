			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean ret;
        HMOrganizationalUnit  hmOrganizationalUnitValidate;
    
        ret = super();
        if(ret)
        {
            if(this.GenderSpecificOccupancy)
            {
                if(this.Gender==HcmPersonGender::None)
                {
                    //ret = false;
                    ret = checkFailed(strfmt("@MAZ1413"));
                }
            }
    
            if(this.IsBlocked)
            {
                if(!this.HMBlockingReasonCode)
                {
                    //ret = false;
                    ret = checkFailed(strfmt("@MAZ1564"));
                }
            }
        }
    
        if(ret && this.OMOperatingUnit)
        {
            select count(RecId) from hmOrganizationalUnitValidate
                where hmOrganizationalUnitValidate.OMOperatingUnit == this.OMOperatingUnit
                && hmOrganizationalUnitValidate.RecId != this.RecId;
    
            if(hmOrganizationalUnitValidate.RecId > 0)
            {
                warning("@MAZ8601");
                ret = false;
            }
        }
    
        return ret;
    }

]]></Source>
