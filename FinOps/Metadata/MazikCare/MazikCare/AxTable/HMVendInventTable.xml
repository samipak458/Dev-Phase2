				<Name>find</Name>
				<Source><![CDATA[
    public static HMVendInventTable find(VendAccount _vendAccount, ItemId _itemid)
    {
        HMVendInventTable hmVendInventTable;
    
        select * from hmVendInventTable
        where hmVendInventTable.VendTable == _vendAccount
        && hmVendInventTable.InventTable == _itemid;
    
        return hmVendInventTable;
    }

]]></Source>
