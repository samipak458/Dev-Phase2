			<Method>
				<Name>validateWrite</Name>
				<Source><![CDATA[
    public boolean validateWrite()
    {
        boolean ret;
        #ISOCountryRegionCodes
    
        ret = super();
    
        // <US>
        if (SysCountryRegionCode::isLegalEntityInCountryRegion([#isoUS]))
        {
            if(this.CreationDate == dateNull())
            {
                warning(strFmt("@SYS84753",fieldPName(HMPatientReferrals, CreationDate)));
                ret = false;
            }
    
            if(!this.InventTable)
            {
                warning(strFmt("@SYS84753",fieldPName(HMPatientReferrals, InventTable)));
                ret = false;
            }
    
            if(!this.ReferralNumber)
            {
                warning(strFmt("@SYS84753",fieldPName(HMPatientReferrals, ReferralNumber)));
                ret = false;
            }
    
            if(ret)
            {
                if(this.ExpiryDate != dateNull() && this.ExpiryDate < this.CreationDate)
                {
                    warning("@MAZ6201");
                    ret = false;
                }
            }
        }
        // </US>
    
        return ret;
    }

]]></Source>
