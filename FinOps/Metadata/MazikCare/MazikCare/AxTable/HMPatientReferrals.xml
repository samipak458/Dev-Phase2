			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds the specified record in the <c>HMPatientReferrals</c> table.
    /// </summary>
    /// <param name="_recId">
    /// The record ID of the record to find.
    /// </param>
    /// <param name="_forUpdate">
    /// A Boolean value that indicates whether to read the record for update; optional.
    /// </param>
    /// <param name="_concurrencyModel">
    /// The concurrency model option to use when reading the record; optional.
    /// </param>
    /// <returns>
    /// A record of the <c>HMPatientReferrals</c> table; otherwise, an empty record.
    /// </returns>
    public static HMPatientReferrals find(
        recId   _recId,
        boolean _forUpdate = false,
        ConcurrencyModel        _concurrencyModel = ConcurrencyModel::Auto)
    {
        HMPatientReferrals    hmPatientReferrals;
    
        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            hmPatientReferrals.concurrencyModel(_concurrencyModel);
        }
    
        if (_recId)
        {
            hmPatientReferrals.selectForUpdate(_forUpdate);
    
            select firstonly hmPatientReferrals
                where hmPatientReferrals.RecId == _recId;
        }
    
        return hmPatientReferrals;
    }

]]></Source>
